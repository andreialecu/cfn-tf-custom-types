    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AuthorizedWriterTeams": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "Team IDs that have write access to this dashboard group. Remember to use an admin's token if using this feature and to include that admin's team (or user id in `authorized_writer_teams`)."
        },
        "AuthorizedWriterUsers": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "User IDs that have write access to this dashboard group. Remember to use an admin's token if using this feature and to include that admin's user id (or team id in `authorized_writer_teams`)."
        },
        "ChartsResolution": {
            "type": "string",
            "description": "Specifies the chart data display resolution for charts in this dashboard. Value can be one of `\"default\"`,  `\"low\"`, `\"high\"`, or  `\"highest\"`."
        },
        "DashboardGroup": {
            "type": "string",
            "description": "The ID of the dashboard group that contains the dashboard."
        },
        "Description": {
            "type": "string",
            "description": "Variable description.\n* `values` - (Optional) List of of strings (which will be treated as an OR filter on the property).\n* `value_required` - (Optional) Determines whether a value is required for this variable (and therefore whether it will be possible to view this dashboard without this filter applied). `false` by default.\n* `values_suggested` - (Optional) A list of strings of suggested values for this variable; these suggestions will receive priority when values are autosuggested for this variable.\n* `restricted_suggestions` - (Optional) If `true`, this variable may only be set to the values listed in `values_suggested` and only these values will appear in autosuggestion menus. `false` by default.\n* `replace_only` - (Optional) If `true`, this variable will only apply to charts that have a filter for the property.\n* `apply_if_exist` - (Optional) If true, this variable will also match data that doesn't have this property at all."
        },
        "DiscoveryOptionsQuery": {
            "type": "string"
        },
        "DiscoveryOptionsSelectors": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "EndTime": {
            "type": "number",
            "description": "Seconds since epoch. Used for visualization."
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "Name of the dashboard."
        },
        "StartTime": {
            "type": "number",
            "description": "Seconds since epoch. Used for visualization."
        },
        "TimeRange": {
            "type": "string",
            "description": "The time range prior to now to visualize. SignalFx time syntax (e.g. `\"-5m\"`, `\"-1h\"`)."
        },
        "Url": {
            "type": "string"
        },
        "Chart": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/ChartDefinition"
            }
        },
        "Column": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/ColumnDefinition"
            }
        },
        "EventOverlay": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/EventOverlayDefinition"
            }
        },
        "Filter": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/FilterDefinition"
            }
        },
        "Grid": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/GridDefinition"
