{
    "typeName": "TF::Vault::CertAuthBackendRole",
    "description": "Provides a resource to create a role in an [Cert auth backend within Vault](https://www.vaultproject.io/docs/auth/cert.html).",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/vault/TF-Vault-CertAuthBackendRole/docs/README.md",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AllowedCommonNames": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "Allowed the common names for authenticated client certificates."
        },
        "AllowedDnsSans": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "Allowed alternative dns names for authenticated client certificates."
        },
        "AllowedEmailSans": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "Allowed emails for authenticated client certificates."
        },
        "AllowedNames": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "Allowed subject names for authenticated client certificates."
        },
        "AllowedOrganizationUnits": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "Allowed organization units for authenticated client certificates."
        },
        "AllowedUriSans": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "Allowed URIs for authenticated client certificates."
        },
        "Backend": {
            "type": "string",
            "description": "Path to the mounted Cert auth backend."
        },
        "BoundCidrs": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "Certificate": {
            "type": "string",
            "description": "CA certificate used to validate client certificates."
        },
        "DisplayName": {
            "type": "string",
            "description": "The name to display on tokens issued under this role."
        },
        "Id": {
            "type": "string"
        },
        "MaxTtl": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "Name of the role."
        },
        "Period": {
            "type": "string"
        },
        "Policies": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "RequiredExtensions": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "TLS extensions required on client certificates."
        },
        "TokenBoundCidrs": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "TokenExplicitMaxTtl": {
            "type": "number"
        },
        "TokenMaxTtl": {
            "type": "number"
        },
        "TokenNoDefaultPolicy": {
            "type": "boolean"
        },
        "TokenNumUses": {
            "type": "number"
        },
        "TokenPeriod": {
            "type": "number"
        },
        "TokenPolicies": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "TokenTtl": {
            "type": "number"
        },
        "TokenType": {
            "type": "string"
        },
        "Ttl": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "Certificate",
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    }
}