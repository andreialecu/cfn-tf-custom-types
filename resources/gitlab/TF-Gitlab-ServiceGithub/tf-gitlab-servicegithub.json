{
    "typeName": "TF::Gitlab::ServiceGithub",
    "description": "**NOTE**: requires either EE (self-hosted) or Silver and above (GitLab.com).\n\nThis resource manages a [GitHub integration](https://docs.gitlab.com/ee/user/project/integrations/github.html) that updates pipeline statuses on a GitHub repo's pull requests.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/gitlab/TF-Gitlab-ServiceGithub/docs/README.md",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Active": {
            "type": "boolean"
        },
        "CreatedAt": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Project": {
            "type": "string",
            "description": "ID of the project you want to activate integration on."
        },
        "RepositoryUrl": {
            "type": "string",
            "description": "The URL of the GitHub repo to integrate with, e,g, https://github.com/gitlabhq/terraform-provider-gitlab."
        },
        "StaticContext": {
            "type": "boolean",
            "description": "Append instance name instead of branch to the status. Must enable to set a GitLab status check as _required_ in GitHub. See [Static / dynamic status check names] to learn more."
        },
        "Title": {
            "type": "string"
        },
        "Token": {
            "type": "string",
            "description": "A GitHub personal access token with at least `repo:status` scope."
        },
        "UpdatedAt": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "Project",
        "RepositoryUrl",
        "Token"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Active",
        "/properties/CreatedAt",
        "/properties/Id",
        "/properties/Title",
        "/properties/UpdatedAt"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    },
    "writeOnlyProperties": [
        "/properties/Token"
    ]
}