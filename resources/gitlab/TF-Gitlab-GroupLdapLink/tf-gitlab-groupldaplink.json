{
    "typeName": "TF::Gitlab::GroupLdapLink",
    "description": "This resource allows you to add an LDAP link to an existing GitLab group.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/gitlab/TF-Gitlab-GroupLdapLink/docs/README.md",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AccessLevel": {
            "type": "string",
            "description": "Acceptable values are: guest, reporter, developer, maintainer, owner."
        },
        "Cn": {
            "type": "string",
            "description": "The CN of the LDAP group to link with."
        },
        "Force": {
            "type": "boolean"
        },
        "GroupId": {
            "type": "string",
            "description": "The id of the GitLab group."
        },
        "Id": {
            "type": "string"
        },
        "LdapProvider": {
            "type": "string",
            "description": "The name of the LDAP provider as stored in the GitLab database."
        }
    },
    "additionalProperties": false,
    "required": [
        "AccessLevel",
        "Cn",
        "GroupId",
        "LdapProvider"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    }
}