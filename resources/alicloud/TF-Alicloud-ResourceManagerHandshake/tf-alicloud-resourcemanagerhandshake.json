{
    "typeName": "TF::Alicloud::ResourceManagerHandshake",
    "description": "Provides a Resource Manager handshake resource. You can invite accounts to join a resource directory for unified management.\nFor information about Resource Manager handshake and how to use it, see [What is Resource Manager handshake](https://www.alibabacloud.com/help/en/doc-detail/135287.htm).\n\n-> **NOTE:** Available in v1.82.0+.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/alicloud/TF-Alicloud-ResourceManagerHandshake/docs/README.md",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "ExpireTime": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "MasterAccountId": {
            "type": "string"
        },
        "MasterAccountName": {
            "type": "string"
        },
        "ModifyTime": {
            "type": "string"
        },
        "Note": {
            "type": "string",
            "description": "Remarks. The maximum length is 1024 characters."
        },
        "ResourceDirectoryId": {
            "type": "string"
        },
        "Status": {
            "type": "string"
        },
        "TargetEntity": {
            "type": "string",
            "description": "Invited account ID or login email."
        },
        "TargetType": {
            "type": "string",
            "description": "Type of account being invited. Valid values: `Account`, `Email`."
        }
    },
    "additionalProperties": false,
    "required": [
        "TargetEntity",
        "TargetType"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/ExpireTime",
        "/properties/Id",
        "/properties/MasterAccountId",
        "/properties/MasterAccountName",
        "/properties/ModifyTime",
        "/properties/ResourceDirectoryId",
        "/properties/Status"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    }
}