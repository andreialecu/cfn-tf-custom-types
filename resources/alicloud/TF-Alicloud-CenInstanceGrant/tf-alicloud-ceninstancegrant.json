        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "CenId": {
            "type": "string",
            "description": "The ID of the CEN."
        },
        "CenOwnerId": {
            "type": "string",
            "description": "The owner UID of the  CEN which the child instance granted to."
        },
        "ChildInstanceId": {
            "type": "string",
            "description": "The ID of the child instance to grant."
