{
    "typeName": "TF::Alicloud::FcFunction",
    "description": "Provides a Alicloud Function Compute Function resource. Function allows you to trigger execution of code in response to events in Alibaba Cloud. The Function itself includes source code and runtime configuration.\n For information about Service and how to use it, see [What is Function Compute](https://www.alibabacloud.com/help/doc-detail/52895.htm).\n\n-> **NOTE:** The resource requires a provider field 'account_id'. [See account_id](https://www.terraform.io/docs/providers/alicloud/index.html#account_id).",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/alicloud/TF-Alicloud-FcFunction/docs/README.md",
    "definitions": {
        "EnvironmentVariablesDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "MapKey": {
                    "type": "string"
                },
                "MapValue": {
                    "type": "string"
                }
            },
            "required": [
                "MapKey",
                "MapValue"
            ]
        },
        "CustomContainerConfigDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Args": {
                    "type": "string"
                },
                "Command": {
                    "type": "string"
                },
                "Image": {
                    "type": "string"
                }
            },
            "required": [
                "Image"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "CaPort": {
            "type": "number"
        },
        "CodeChecksum": {
            "type": "string",
            "description": "The checksum (crc64) of the function code.The value can be generated by data source [alicloud_file_crc64_checksum](https://www.terraform.io/docs/providers/alicloud/d/file_crc64_checksum.html).\n-> **NOTE:** For more information, see [Limits](https://www.alibabacloud.com/help/doc-detail/51907.htm)."
        },
        "Description": {
            "type": "string",
            "description": "The Function Compute function description."
        },
        "EnvironmentVariables": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/EnvironmentVariablesDefinition"
            },
            "description": "A map that defines environment variables for the function."
        },
        "Filename": {
            "type": "string",
            "description": "The path to the function's deployment package within the local filesystem. It is conflict with the `oss_`-prefixed options."
        },
        "FunctionId": {
            "type": "string"
        },
        "Handler": {
            "type": "string",
            "description": "The function [entry point](https://www.alibabacloud.com/help/doc-detail/62213.htm) in your code."
        },
        "Id": {
            "type": "string"
        },
        "InitializationTimeout": {
            "type": "number"
        },
        "Initializer": {
            "type": "string"
        },
        "InstanceConcurrency": {
            "type": "number"
        },
        "InstanceType": {
            "type": "string"
        },
        "LastModified": {
            "type": "string"
        },
        "MemorySize": {
            "type": "number",
            "description": "Amount of memory in MB your Function can use at runtime. Defaults to `128`. Limits to [128, 3072]."
        },
        "Name": {
            "type": "string",
            "description": "The Function Compute function name. It is the only in one service and is conflict with \"name_prefix\"."
        },
        "NamePrefix": {
            "type": "string",
            "description": "Setting a prefix to get a only function name. It is conflict with \"name\"."
        },
        "OssBucket": {
            "type": "string",
            "description": "The OSS bucket location containing the function's deployment package. Conflicts with `filename`. This bucket must reside in the same Alibaba Cloud region where you are creating the function."
        },
        "OssKey": {
            "type": "string",
            "description": "The OSS key of an object containing the function's deployment package. Conflicts with `filename`."
        },
        "Runtime": {
            "type": "string",
            "description": "See [Runtimes][https://www.alibabacloud.com/help/doc-detail/52077.htm] for valid values."
        },
        "Service": {
            "type": "string",
            "description": "The Function Compute service name."
        },
        "Timeout": {
            "type": "number",
            "description": "The amount of time your Function has to run in seconds."
        },
        "CustomContainerConfig": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/CustomContainerConfigDefinition"
            },
            "maxItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "Handler",
        "Runtime",
        "Service"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/FunctionId",
        "/properties/Id",
        "/properties/LastModified"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    }
}