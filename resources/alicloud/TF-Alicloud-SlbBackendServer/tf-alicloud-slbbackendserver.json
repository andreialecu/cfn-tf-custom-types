{
    "typeName": "TF::Alicloud::SlbBackendServer",
    "description": "Add a group of backend servers (ECS or ENI instance) to the Server Load Balancer or remove them from it.\n\n-> **NOTE:** Available in 1.53.0+",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/alicloud/TF-Alicloud-SlbBackendServer/docs/README.md",
    "definitions": {
        "BackendServersDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "ServerId": {
                    "type": "string"
                },
                "ServerIp": {
                    "type": "string"
                },
                "Type": {
                    "type": "string"
                },
                "Weight": {
                    "type": "number"
                }
            },
            "required": [
                "ServerId",
                "Weight"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "DeleteProtectionValidation": {
            "type": "boolean",
            "description": "Checking DeleteProtection of SLB instance before deleting. If true, this resource will not be deleted when its SLB instance enabled DeleteProtection. Default to false."
        },
        "Id": {
            "type": "string"
        },
        "LoadBalancerId": {
            "type": "string",
            "description": "ID of the load balancer."
        },
        "BackendServers": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/BackendServersDefinition"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "LoadBalancerId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    }
}