            "type": "string"
        },
        "OrderType": {
            "type": "string"
        },
        "Password": {
            "type": "string",
            "description": "The password of the DB instance. The password is a string of 8 to 30 characters and must contain uppercase letters, lowercase letters, and numbers."
        },
        "PaymentType": {
            "type": "string"
        },
        "Period": {
            "type": "string",
