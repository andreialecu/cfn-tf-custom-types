            "type": "string"
        },
        "PrivateConnectionPrefix": {
            "type": "string"
        },
        "PrivateIp": {
            "type": "string",
            "description": "Set the instance's private IP."
        },
        "Qps": {
            "type": "number"
        },
        "ResourceGroupId": {
            "type": "string",
            "description": "The ID of resource group which the resource belongs."
        },
        "RestoreTime": {
            "type": "string"
        },
        "SecurityGroupId": {
            "type": "string",
            "description": "The Security Group ID of ECS."
        },
        "SecurityIpGroupAttribute": {
            "type": "string"
        },
        "SecurityIpGroupName": {
            "type": "string"
        },
        "SecurityIps": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "Set the instance's IP whitelist of the default security group."
        },
        "SrcdbInstanceId": {
            "type": "string"
        },
        "SslEnable": {
            "type": "string"
        },
        "Status": {
            "type": "string"
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/TagsDefinition"
            },
            "description": "A mapping of tags to assign to the resource."
        },
        "VpcAuthMode": {
            "type": "string",
            "description": "Only meaningful if instance_type is `Redis` and network type is VPC. Valid values are `Close`, `Open`. Defaults to `Open`.  `Close` means the redis instance can be accessed without authentication. `Open` means authentication is required."
        },
        "VswitchId": {
            "type": "string",
            "description": "The ID of VSwitch."
        },
        "ZoneId": {
            "type": "string"
        },
        "Parameters": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/ParametersDefinition"
            }
        },
        "Timeouts": {
