        },
        "EndTime": {
            "type": "string"
        },
        "EngineVersion": {
            "type": "string",
            "description": "Engine version. Supported values: 2.8, 4.0 and 5.0. Default value: 2.8. Only 2.8 can be supported for Memcache Instance."
        },
        "ForceUpgrade": {
            "type": "boolean"
        },
        "GlobalInstance": {
            "type": "boolean"
        },
