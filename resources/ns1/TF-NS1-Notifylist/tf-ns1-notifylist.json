{
    "typeName": "TF::NS1::Notifylist",
    "description": "Provides a NS1 Notify List resource. This can be used to create, modify, and delete notify lists.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/ns1/TF-NS1-Notifylist/docs/README.md",
    "definitions": {
        "NotificationsDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Config": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/ConfigDefinition"
                    },
                    "description": "Configuration details for the given notifier type."
                },
                "Type": {
                    "type": "string",
                    "description": "The type of notifier. Available notifiers are indicated in /notifytypes endpoint."
                }
            },
            "required": [
                "Config",
                "Type"
            ]
        },
        "ConfigDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "MapKey": {
                    "type": "string"
                },
                "MapValue": {
                    "type": "string"
                }
            },
            "required": [
                "MapKey",
                "MapValue"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The free-form display name for the notify list."
        },
        "Notifications": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/NotificationsDefinition"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
