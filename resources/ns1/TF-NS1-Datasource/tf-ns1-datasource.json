{
    "typeName": "TF::NS1::Datasource",
    "description": "Provides a NS1 Data Source resource. This can be used to create, modify, and delete data sources.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/ns1/TF-NS1-Datasource/docs/README.md",
    "definitions": {
        "ConfigDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "MapKey": {
                    "type": "string"
                },
                "MapValue": {
                    "type": "string"
                }
            },
            "required": [
                "MapKey",
                "MapValue"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Config": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/ConfigDefinition"
            },
            "description": "The data source configuration, determined by its type,\nmatching the specification in `config` from /data/sourcetypes."
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The free form name of the data source."
        },
        "Sourcetype": {
            "type": "string",
            "description": "The data sources type, listed in API endpoint https://api.nsone.net/v1/data/sourcetypes."
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "Sourcetype"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    }
}