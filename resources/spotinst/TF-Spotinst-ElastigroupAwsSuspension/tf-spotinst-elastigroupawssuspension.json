    "definitions": {
        "SuspensionDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Name": {
                    "type": "string"
                }
            },
            "required": [
                "Name"
            ]
        }
    },
    "properties": {
