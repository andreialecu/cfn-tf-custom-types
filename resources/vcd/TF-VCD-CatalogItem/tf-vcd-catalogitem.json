{
    "typeName": "TF::VCD::CatalogItem",
    "description": "Provides a vCloud Director catalog item resource. This can be used to upload OVA to catalog and delete it.\n\nSupported in provider *v2.0+*",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/vcd/TF-VCD-CatalogItem/docs/README.md",
    "definitions": {
        "MetadataDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "MapKey": {
                    "type": "string"
                },
                "MapValue": {
                    "type": "string"
                }
            },
            "required": [
                "MapKey",
                "MapValue"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Catalog": {
            "type": "string",
            "description": "The name of the catalog where to upload OVA file."
        },
        "Created": {
            "type": "string"
        },
        "Description": {
            "type": "string",
            "description": "- Description of item."
        },
        "Id": {
            "type": "string"
        },
        "Metadata": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/MetadataDefinition"
            },
            "description": "Key value map of metadata to assign."
        },
        "Name": {
            "type": "string",
            "description": "Item name in catalog."
        },
        "Org": {
            "type": "string",
            "description": "The name of organization to use, optional if defined at provider level. Useful when connected as sysadmin working across different organisations."
        },
        "OvaPath": {
            "type": "string",
            "description": "- Absolute or relative path to file to upload."
        },
        "ShowUploadProgress": {
            "type": "boolean",
            "description": "- Default false. Allows to see upload progress."
        },
        "UploadPieceSize": {
            "type": "number",
            "description": "- Size in MB for splitting upload size. It can possibly impact upload performance. Default 1MB."
        }
    },
    "additionalProperties": false,
    "required": [
        "Catalog",
        "Name",
        "OvaPath"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Created",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    }
}