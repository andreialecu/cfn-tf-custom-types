{
    "typeName": "TF::VCD::VappStaticRouting",
    "description": "Provides a vCloud Director vApp static routing resource. This can be used to create,\nmodify, and delete static routing rules in a [vApp network](/docs/providers/vcd/r/vapp_network.html).\n\n~> **Note:** Resource used for vApp network, not vApp Org network.\n\n!> **Warning:** Using this resource overrides any existing static routing rules on vApp network. It's recommended to have only one resource per vApp and vApp network.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/vcd/TF-VCD-VappStaticRouting/docs/README.md",
    "definitions": {
        "RuleDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Name": {
                    "type": "string"
                },
                "NetworkCidr": {
                    "type": "string"
                },
                "NextHopIp": {
                    "type": "string"
                }
            },
            "required": [
                "Name",
                "NetworkCidr",
                "NextHopIp"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Enabled": {
            "type": "boolean",
            "description": "Enable or disable static Routing. Default is `true`."
        },
        "Id": {
            "type": "string"
        },
        "NetworkId": {
            "type": "string",
            "description": "The identifier of [vApp network](/docs/providers/vcd/r/vapp_network.html)."
        },
        "Org": {
            "type": "string",
            "description": "The name of organization to use, optional if defined at provider level. Useful when connected as sysadmin working across different organisations."
        },
        "VappId": {
            "type": "string",
            "description": "The identifier of [vApp](/docs/providers/vcd/r/vapp.html)."
        },
        "Vdc": {
            "type": "string",
            "description": "The name of VDC to use, optional if defined at provider level."
        },
        "Rule": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/RuleDefinition"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "NetworkId",
        "VappId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    }
}