{
    "typeName": "TF::VCD::NsxtEdgegateway",
    "description": "Provides a VMware Cloud Director NSX-T edge gateway. This can be used to create, update, and delete NSX-T edge gateways connected\nto external networks.\n\n~> **Note:** Only `System Administrator` can create an edge gateway.\nYou must use `System Adminstrator` account in `provider` configuration\nand then provide `org` and `vdc` arguments for edge gateway to work.\n\n-> **Note:** This resource uses new VMware Cloud Director\n[OpenAPI](https://code.vmware.com/docs/11982/getting-started-with-vmware-cloud-director-openapi) and\nrequires at least VCD *10.1.1+* and NSX-T *3.0+*.\n\nSupported in provider *v3.1+*.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/vcd/TF-VCD-NsxtEdgegateway/docs/README.md",
    "definitions": {
        "SubnetDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Gateway": {
                    "type": "string"
                },
                "PrefixLength": {
                    "type": "number"
                },
                "PrimaryIp": {
                    "type": "string"
                },
                "AllocatedIps": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "$ref": "#/definitions/AllocatedIpsDefinition"
                    },
                    "minItems": 1
                }
            },
            "required": [
                "Gateway",
                "PrefixLength"
            ]
        },
        "AllocatedIpsDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "EndAddress": {
                    "type": "string"
                },
                "StartAddress": {
                    "type": "string"
                }
            },
            "required": [
                "EndAddress",
                "StartAddress"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "DedicateExternalNetwork": {
            "type": "boolean",
            "description": "Dedicating the External Network will enable Route Advertisement for this Edge Gateway. Default `false`."
        },
        "Description": {
            "type": "string",
            "description": "A unique name for the edge gateway."
        },
        "EdgeClusterId": {
            "type": "string",
            "description": "Specific Edge Cluster ID if required."
        },
        "ExternalNetworkId": {
            "type": "string",
            "description": "An external network ID. **Note.** Data source [vcd_external_network_v2](/docs/providers/vcd/d/external_network_v2.html)\ncan be used to lookup ID by name."
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "A unique name for the edge gateway."
        },
        "Org": {
            "type": "string",
            "description": "The name of organization to which the VDC belongs. Optional if defined at provider level."
        },
        "PrimaryIp": {
            "type": "string"
        },
        "Vdc": {
            "type": "string",
            "description": "The name of VDC that owns the edge gateway. Optional if defined at provider level."
        },
        "Subnet": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/SubnetDefinition"
            },
            "minItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "ExternalNetworkId",
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id",
        "/properties/PrimaryIp"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    }
}