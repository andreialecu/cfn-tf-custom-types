{
    "typeName": "TF::VCD::Org",
    "description": "Provides a vCloud Director Org resource. This can be used to create, update, and delete an organization.\nRequires system administrator privileges.\n\nSupported in provider *v2.0+*",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/vcd/TF-VCD-Org/docs/README.md",
    "definitions": {
        "VappLeaseDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "DeleteOnStorageLeaseExpiration": {
                    "type": "boolean"
                },
                "MaximumRuntimeLeaseInSec": {
                    "type": "number"
                },
                "MaximumStorageLeaseInSec": {
                    "type": "number"
                },
                "PowerOffOnRuntimeLeaseExpiration": {
                    "type": "boolean"
                }
            },
            "required": [
                "DeleteOnStorageLeaseExpiration",
                "MaximumRuntimeLeaseInSec",
                "MaximumStorageLeaseInSec",
                "PowerOffOnRuntimeLeaseExpiration"
            ]
        },
        "VappTemplateLeaseDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "DeleteOnStorageLeaseExpiration": {
                    "type": "boolean"
                },
                "MaximumStorageLeaseInSec": {
                    "type": "number"
                }
            },
            "required": [
                "DeleteOnStorageLeaseExpiration",
                "MaximumStorageLeaseInSec"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "CanPublishCatalogs": {
            "type": "boolean",
            "description": "- True if this organization is allowed to share catalogs. Default is `true`."
        },
        "DelayAfterPowerOnSeconds": {
            "type": "number",
            "description": "- Specifies this organization's default for virtual machine boot delay after power on. Default is `0`."
        },
        "DeleteForce": {
            "type": "boolean",
            "description": "- pass `delete_force=true` and `delete_recursive=true` to remove an organization or VDC and any objects it contains, regardless of their state."
        },
        "DeleteRecursive": {
            "type": "boolean",
            "description": "- pass `delete_recursive`=true as query parameter to remove an organization or VDC and any objects it contains that are in a state that normally allows removal."
        },
        "DeployedVmQuota": {
            "type": "number",
            "description": "- Maximum number of virtual machines that can be deployed simultaneously by a member of this organization. Default is unlimited (0)."
        },
        "Description": {
            "type": "string",
            "description": "- Org description. Default is empty."
        },
        "FullName": {
            "type": "string",
            "description": "Org full name."
        },
        "Id": {
            "type": "string"
        },
        "IsEnabled": {
            "type": "boolean",
            "description": "- True if this organization is enabled (allows login and all other operations). Default is `true`."
        },
        "Name": {
            "type": "string",
            "description": "Org name."
        },
        "StoredVmQuota": {
            "type": "number",
            "description": "- Maximum number of virtual machines in vApps or vApp templates that can be stored in an undeployed state by a member of this organization. Default is unlimited (0)."
        },
        "VappLease": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/VappLeaseDefinition"
            },
            "maxItems": 1
        },
        "VappTemplateLease": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/VappTemplateLeaseDefinition"
            },
            "maxItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "DeleteForce",
        "DeleteRecursive",
        "FullName",
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    }
}