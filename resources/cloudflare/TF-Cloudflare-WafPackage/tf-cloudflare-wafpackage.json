{
    "typeName": "TF::Cloudflare::WafPackage",
    "description": "Provides a Cloudflare WAF rule package resource for a particular zone. This can be used to configure firewall behaviour for pre-defined firewall packages.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/cloudflare/TF-Cloudflare-WafPackage/docs/README.md",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "ActionMode": {
            "type": "string",
            "description": "The action mode of the package, can be one of [\"block\", \"challenge\", \"simulate\"]."
        },
        "Id": {
            "type": "string"
        },
        "PackageId": {
            "type": "string",
            "description": "The WAF Package ID."
        },
        "Sensitivity": {
            "type": "string",
            "description": "The sensitivity of the package, can be one of [\"high\", \"medium\", \"low\", \"off\"]."
        },
        "ZoneId": {
            "type": "string",
            "description": "The DNS zone ID to apply to."
        }
    },
    "additionalProperties": false,
    "required": [
        "PackageId",
        "ZoneId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
