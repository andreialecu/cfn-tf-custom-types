{
    "typeName": "TF::Cloudflare::LogpullRetention",
    "description": "Allows management of the Logpull Retention settings used to control whether or not to retain HTTP request logs.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/cloudflare/TF-Cloudflare-LogpullRetention/docs/README.md",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Enabled": {
            "type": "boolean",
            "description": "Whether you wish to retain logs or not."
        },
        "Id": {
            "type": "string"
        },
        "ZoneId": {
            "type": "string",
            "description": "The zone ID to apply the log retention to."
        }
    },
    "additionalProperties": false,
    "required": [
        "Enabled",
        "ZoneId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
