    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/cloudflare/TF-Cloudflare-RateLimit/docs/README.md",
    "definitions": {
        "ActionDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Mode": {
                    "type": "string"
                },
                "Timeout": {
                    "type": "number"
                },
                "Response": {
                    "type": "array",
