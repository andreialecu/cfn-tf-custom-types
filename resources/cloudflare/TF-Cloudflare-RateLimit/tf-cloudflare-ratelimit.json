                "Headers": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "type": "array",
                        "insertionOrder": true,
                        "items": {
                            "$ref": "#/definitions/HeadersDefinition"
                        }
                    }
                },
                "OriginTraffic": {
                    "type": "boolean"
                },
                "Statuses": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "number"
                    }
                }
            },
            "required": []
        },
        "HeadersDefinition": {
            "type": "object",
