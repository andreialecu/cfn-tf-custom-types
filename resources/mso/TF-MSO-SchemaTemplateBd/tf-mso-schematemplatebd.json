            "type": "boolean"
        },
        "Layer2Stretch": {
            "type": "boolean"
        },
        "Layer2UnknownUnicast": {
            "type": "string"
        },
        "Layer3Multicast": {
            "type": "boolean"
        },
        "Name": {
            "type": "string"
        },
        "OptimizeWanBandwidth": {
            "type": "boolean"
        },
        "SchemaId": {
            "type": "string"
        },
        "TemplateName": {
            "type": "string"
        },
        "VrfName": {
            "type": "string"
        },
        "VrfSchemaId": {
            "type": "string"
        },
        "VrfTemplateName": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "DisplayName",
        "Name",
        "SchemaId",
        "TemplateName",
        "VrfName"
