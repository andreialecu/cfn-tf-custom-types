                "$ref": "#/definitions/LocationDefinition"
            }
        },
        "LoginDomain": {
            "type": "string"
        },
        "MaintenanceMode": {
            "type": "boolean"
        },
        "Name": {
            "type": "string"
        },
        "Password": {
            "type": "string"
        },
        "Platform": {
            "type": "string"
        },
        "Urls": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "Username": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "ApicSiteId",
        "Name",
        "Password",
        "Urls",
        "Username"
