        "ApicSiteId": {
            "type": "string"
        },
        "CloudProviders": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "Id": {
            "type": "string"
        },
        "Labels": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "Location": {
            "type": "array",
            "insertionOrder": true,
