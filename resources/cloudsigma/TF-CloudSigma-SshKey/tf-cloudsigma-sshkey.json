{
    "typeName": "TF::CloudSigma::SshKey",
    "description": "Provides a CloudSigma SSH key resource. to allow you to manage SSH keys. Keys\ncreated with this resource can be referenced in your server configuration via\ntheir IDs.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/cloudsigma/TF-CloudSigma-SshKey/docs/README.md",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The name of the SSH key for identification."
        },
        "PrivateKey": {
            "type": "string",
            "description": "The private key. If this is a file, it can be read using the file interpolation function."
        },
        "PublicKey": {
            "type": "string",
            "description": "The public key. If this is a file, it can be read using the file interpolation function."
        },
        "Uuid": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id",
        "/properties/Uuid"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
