    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/google/TF-Google-StorageTransferJob/docs/README.md",
    "definitions": {
        "ScheduleDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "ScheduleEndDate": {
