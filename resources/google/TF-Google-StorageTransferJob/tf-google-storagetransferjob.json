        "ScheduleEndDateDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Day": {
                    "type": "number",
                    "description": "Day of month. Must be from 1 to 31 and valid for the year and month."
                },
                "Month": {
                    "type": "number",
                    "description": "Month of year. Must be from 1 to 12."
                },
                "Year": {
                    "type": "number",
                    "description": "Year of date. Must be from 1 to 9999."
                }
            },
            "required": [
                "Day",
                "Month",
                "Year"
            ]
        },
        "ScheduleStartDateDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Day": {
                    "type": "number"
                },
                "Month": {
                    "type": "number"
                },
                "Year": {
                    "type": "number"
                }
            },
            "required": [
                "Day",
                "Month",
                "Year"
            ]
        },
        "StartTimeOfDayDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Hours": {
                    "type": "number",
                    "description": "Hours of day in 24 hour format. Should be from 0 to 23."
                },
                "Minutes": {
                    "type": "number",
                    "description": "Minutes of hour of day. Must be from 0 to 59."
                },
                "Nanos": {
                    "type": "number",
                    "description": "Fractions of seconds in nanoseconds. Must be from 0 to 999,999,999."
                },
                "Seconds": {
                    "type": "number",
                    "description": "Seconds of minutes of the time. Must normally be from 0 to 59."
                }
            },
            "required": [
                "Hours",
                "Minutes",
                "Nanos",
                "Seconds"
            ]
        },
        "AwsS3DataSourceDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "BucketName": {
                    "type": "string",
                    "description": "S3 Bucket name."
                },
