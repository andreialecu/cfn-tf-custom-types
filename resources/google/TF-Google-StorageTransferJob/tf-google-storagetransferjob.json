                        "$ref": "#/definitions/AwsS3DataSourceDefinition"
                    },
                    "maxItems": 1
                },
                "AzureBlobStorageDataSource": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/AzureBlobStorageDataSourceDefinition"
                    },
                    "maxItems": 1
                },
                "GcsDataSink": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/GcsDataSinkDefinition"
                    },
                    "maxItems": 1
