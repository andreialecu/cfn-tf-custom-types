                    },
                    "maxItems": 1,
                    "minItems": 1
                },
                "StartTimeOfDay": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/StartTimeOfDayDefinition"
                    },
                    "maxItems": 1
                }
            },
            "required": []
        },
