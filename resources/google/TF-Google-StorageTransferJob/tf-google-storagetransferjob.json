            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Container": {
                    "type": "string",
                    "description": "The container to transfer from the Azure Storage account.`."
                },
                "Path": {
                    "type": "string",
                    "description": "Root path to transfer objects. Must be an empty string or full path name that ends with a '/'. This field is treated as an object prefix. As such, it should generally not begin with a '/'."
                },
                "StorageAccount": {
                    "type": "string",
                    "description": "The name of the Azure Storage account."
                },
                "AzureCredentials": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/AzureCredentialsDefinition"
                    },
                    "maxItems": 1,
                    "minItems": 1
                }
            },
            "required": [
                "Container",
                "StorageAccount"
            ]
        },
        "GcsDataSinkDefinition": {
            "type": "object",
