{
    "typeName": "TF::Akamai::NetworklistSubscription",
    "description": "Use the `akamai_networklist_subscription` resource to specify a set of email addresses to be notified of changes to any\nof a set of network lists.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/akamai/TF-Akamai-NetworklistSubscription/docs/README.md",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "NetworkList": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "A list containing one or more IDs of the network lists to which the indicated email\naddresses should be subscribed."
        },
        "Recipients": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "A bracketed, comma-separated list of email addresses that will be notified of changes to any\nof the specified network lists."
        }
    },
    "additionalProperties": false,
    "required": [
        "NetworkList",
        "Recipients"
    ],
    "readOnlyProperties": [
