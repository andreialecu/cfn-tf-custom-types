            "additionalProperties": false,
            "properties": {
                "DatacenterId": {
                    "type": "number"
                },
                "Nickname": {
                    "type": "string"
                }
            }
        },
        "AssignmentDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
