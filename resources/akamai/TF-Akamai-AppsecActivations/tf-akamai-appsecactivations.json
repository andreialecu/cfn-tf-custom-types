{
    "typeName": "TF::Akamai::AppsecActivations",
    "description": "The `akamai_appsec_activations` resource allows you to activate or deactivate a given security configuration version.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/akamai/TF-Akamai-AppsecActivations/docs/README.md",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Activate": {
            "type": "boolean",
            "description": "A boolean indicating whether to activate the specified configuration version. If not supplied, True is assumed."
        },
        "ConfigId": {
            "type": "number",
            "description": "The ID of the security configuration to use."
        },
        "Id": {
            "type": "string"
        },
        "Network": {
            "type": "string",
            "description": "The network in which the security configuration should be activated. If supplied, must be either STAGING or PRODUCTION. If not supplied, STAGING will be assumed."
        },
        "Notes": {
            "type": "string",
            "description": "An optional text note describing this operation."
        },
        "NotificationEmails": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "A bracketed, comma-separated list of email addresses that will be notified when the operation is complete."
        },
        "Status": {
            "type": "string"
        },
        "Version": {
            "type": "number",
            "description": "The version number of the security configuration to use."
        }
    },
    "additionalProperties": false,
    "required": [
        "ConfigId",
        "NotificationEmails",
        "Version"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id",
        "/properties/Status"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    }
}