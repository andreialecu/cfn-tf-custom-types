{
    "typeName": "TF::Akamai::AppsecIpGeo",
    "description": "Use the `akamai_appsec_ip_geo` resource to update the method and which network lists to use for IP/Geo firewall blocking.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/akamai/TF-Akamai-AppsecIpGeo/docs/README.md",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "ConfigId": {
            "type": "number",
            "description": "The ID of the security configuration to use."
        },
        "ExceptionIpNetworkLists": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "The network lists to be allowed regardless of `mode`, `geo_network_lists`, and `ip_network_lists` parameters."
        },
        "GeoNetworkLists": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "The network lists to be blocked or allowed geographically."
        },
        "Id": {
            "type": "string"
        },
        "IpNetworkLists": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "The network lists to be blocked or allowd by IP address."
        },
        "Mode": {
            "type": "string",
            "description": "The mode to use for IP/Geo firewall blocking: `block` to block specific IPs, geographies or network lists, or `allow` to allow specific IPs or geographies to be let through while blocking the rest."
        },
        "SecurityPolicyId": {
            "type": "string",
            "description": "The ID of the security policy to use."
        },
        "Version": {
            "type": "number",
            "description": "The version number of the security configuration to use."
        }
    },
    "additionalProperties": false,
    "required": [
        "ConfigId",
        "Mode",
        "SecurityPolicyId",
        "Version"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
