        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Activate": {
            "type": "boolean"
        },
        "Id": {
            "type": "string"
        },
        "Network": {
            "type": "string",
            "description": "The network to be used, either `STAGING` or `PRODUCTION`. If not supplied, defaults to\n`STAGING`."
        },
        "NetworkListId": {
            "type": "string",
            "description": "The ID of the network list to be activated."
        },
        "Notes": {
            "type": "string",
