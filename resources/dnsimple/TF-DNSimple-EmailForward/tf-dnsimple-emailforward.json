    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AliasName": {
            "type": "string",
            "description": "The name part (the part before the @) of the source email address on the domain."
        },
        "DestinationEmail": {
            "type": "string",
            "description": "The destination email address on another domain."
        },
        "Domain": {
            "type": "string",
            "description": "The domain to add the email forwarding rule to."
        },
        "Id": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "AliasName",
        "DestinationEmail",
        "Domain"
    ],
    "readOnlyProperties": [
