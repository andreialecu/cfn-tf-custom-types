{
    "typeName": "TF::ProfitBricks::K8sCluster",
    "description": "Manages a Managed Kubernetes cluster on ProfitBricks.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/profitbricks/TF-ProfitBricks-K8sCluster/docs/README.md",
    "definitions": {
        "MaintenanceWindowDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "DayOfTheWeek": {
                    "type": "string"
                },
                "Time": {
                    "type": "string"
                }
            },
            "required": [
                "DayOfTheWeek",
                "Time"
            ]
        },
        "TimeoutsDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Default": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "K8sVersion": {
            "type": "string",
            "description": "[string] The desired Kubernetes Version. For supported values, please check the API documentation.\n- `maintenance_window` - (Optional) See the **maintenance_window** section in the example above."
        },
        "Name": {
            "type": "string",
            "description": "[string] The name of the Kubernetes Cluster.\n- `k8s_version` - (Optional)[string] The desired Kubernetes Version. For supported values, please check the API documentation.\n- `maintenance_window` - (Optional) See the **maintenance_window** section in the example above."
        },
        "MaintenanceWindow": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/MaintenanceWindowDefinition"
            },
            "maxItems": 1
        },
        "Timeouts": {
            "$ref": "#/definitions/TimeoutsDefinition"
        }
    },
    "additionalProperties": false,
    "required": [
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    }
}