{
    "typeName": "TF::Splunk::ConfigsConf",
    "description": "Create and manage configuration file stanzas.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/splunk/TF-Splunk-ConfigsConf/docs/README.md",
    "definitions": {
        "VariablesDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "MapKey": {
                    "type": "string"
                },
                "MapValue": {
                    "type": "string"
                }
            },
            "required": [
                "MapKey",
                "MapValue"
            ]
        },
        "AclDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "App": {
                    "type": "string"
                },
                "CanChangePerms": {
                    "type": "boolean"
                },
                "CanShareApp": {
                    "type": "boolean"
                },
                "CanShareGlobal": {
                    "type": "boolean"
                },
                "CanShareUser": {
                    "type": "boolean"
                },
                "CanWrite": {
                    "type": "boolean"
                },
                "Owner": {
                    "type": "string"
                },
                "Read": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "type": "string"
                    }
                },
                "Removable": {
                    "type": "boolean"
                },
                "Sharing": {
                    "type": "string"
                },
                "Write": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "type": "string"
                    }
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "A '/' separated string consisting of {conf_file_name}/{stanza_name} ex. props/custom_stanza."
        },
        "Variables": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/VariablesDefinition"
            },
            "description": "A map of key value pairs for a stanza."
        },
        "Acl": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/AclDefinition"
            },
            "maxItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    }
}