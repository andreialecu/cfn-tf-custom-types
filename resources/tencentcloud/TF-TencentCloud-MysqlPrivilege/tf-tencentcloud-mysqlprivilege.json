{
    "typeName": "TF::TencentCloud::MysqlPrivilege",
    "description": "Provides a mysql account privilege resource to grant different access privilege to different database. A database can be granted by multiple account.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/tencentcloud/TF-TencentCloud-MysqlPrivilege/docs/README.md",
    "definitions": {
        "ColumnDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "ColumnName": {
                    "type": "string",
                    "description": "Column name."
                },
                "DatabaseName": {
                    "type": "string",
                    "description": "Database name."
                },
                "Privileges": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    },
                    "description": "Column privilege.available values for Privileges:SELECT,INSERT,UPDATE,REFERENCES."
                },
                "TableName": {
                    "type": "string",
                    "description": "Table name."
                }
            },
            "required": [
                "ColumnName",
                "DatabaseName",
                "Privileges",
                "TableName"
            ]
        },
        "DatabaseDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "DatabaseName": {
                    "type": "string",
                    "description": "Database name."
                },
                "Privileges": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    },
                    "description": "Database privilege.available values for Privileges:SELECT,INSERT,UPDATE,DELETE,CREATE,DROP,REFERENCES,INDEX,ALTER,CREATE TEMPORARY TABLES,LOCK TABLES,EXECUTE,CREATE VIEW,SHOW VIEW,CREATE ROUTINE,ALTER ROUTINE,EVENT,TRIGGER."
                }
            },
            "required": [
                "DatabaseName",
                "Privileges"
            ]
        },
        "TableDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "DatabaseName": {
                    "type": "string",
                    "description": "Database name."
                },
                "Privileges": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    },
                    "description": "Table privilege.available values for Privileges:SELECT,INSERT,UPDATE,DELETE,CREATE,DROP,REFERENCES,INDEX,ALTER,CREATE VIEW,SHOW VIEW,TRIGGER."
                },
                "TableName": {
                    "type": "string",
                    "description": "Table name."
                }
            },
            "required": [
                "DatabaseName",
                "Privileges",
                "TableName"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AccountHost": {
            "type": "string",
            "description": "Account host, default is `%`."
        },
        "AccountName": {
            "type": "string",
            "description": "Account name.the forbidden value is:root,mysql.sys,tencentroot."
        },
        "Global": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "Global privileges. available values for Privileges:SELECT,INSERT,UPDATE,DELETE,CREATE,PROCESS,DROP,REFERENCES,INDEX,ALTER,SHOW DATABASES,CREATE TEMPORARY TABLES,LOCK TABLES,EXECUTE,CREATE VIEW,SHOW VIEW,CREATE ROUTINE,ALTER ROUTINE,EVENT,TRIGGER."
        },
        "Id": {
            "type": "string"
        },
        "MysqlId": {
            "type": "string",
            "description": "Instance ID."
        },
        "Column": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/ColumnDefinition"
            }
        },
        "Database": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/DatabaseDefinition"
            }
        },
        "Table": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/TableDefinition"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "AccountName",
        "Global",
        "MysqlId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
