{
    "typeName": "TF::TencentCloud::RedisBackupConfig",
    "description": "Use this resource to create a backup config of redis.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/tencentcloud/TF-TencentCloud-RedisBackupConfig/docs/README.md",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "BackupPeriod": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "Specifys which day the backup action should take place. Valid values: `Monday`, `Tuesday`, `Wednesday`, `Thursday`, `Friday`, `Saturday` and `Sunday`."
        },
        "BackupTime": {
            "type": "string",
            "description": "Specifys what time the backup action should take place. And the time interval should be one hour."
        },
        "Id": {
            "type": "string"
        },
        "RedisId": {
            "type": "string",
            "description": "ID of a redis instance to which the policy will be applied."
        }
    },
    "additionalProperties": false,
    "required": [
        "BackupPeriod",
        "BackupTime",
        "RedisId"
    ],
    "readOnlyProperties": [
