{
    "typeName": "TF::UpCloud::Network",
    "description": "CloudFormation equivalent of upcloud_network",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/upcloud/TF-UpCloud-Network/docs/README.md",
    "definitions": {
        "ServersDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Id": {
                    "type": "string"
                },
                "Title": {
                    "type": "string"
                }
            }
        },
        "IpNetworkDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Address": {
                    "type": "string"
                },
                "Dhcp": {
                    "type": "boolean"
                },
                "DhcpDefaultRoute": {
                    "type": "boolean"
                },
                "DhcpDns": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    }
                },
                "Family": {
                    "type": "string"
                },
                "Gateway": {
                    "type": "string"
                }
            },
            "required": [
                "Address",
                "Dhcp",
                "Family"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "Router": {
            "type": "string"
        },
        "Servers": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/ServersDefinition"
            }
        },
        "Type": {
            "type": "string"
        },
        "Zone": {
            "type": "string"
        },
        "IpNetwork": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/IpNetworkDefinition"
            },
            "maxItems": 1,
            "minItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "Zone"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id",
        "/properties/Servers",
        "/properties/Type"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    }
}