        },
        "Kubeconfig": {
            "type": "string"
        },
        "Label": {
            "type": "string",
            "description": "This Kubernetes cluster's unique label."
        },
        "Region": {
            "type": "string",
            "description": "This Kubernetes cluster's location."
        },
        "Status": {
            "type": "string"
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
