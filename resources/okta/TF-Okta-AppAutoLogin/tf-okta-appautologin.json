{
    "typeName": "TF::Okta::AppAutoLogin",
    "description": "Creates an Auto Login Okta Application.\n\nThis resource allows you to create and configure an Auto Login Okta Application.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/okta/TF-Okta-AppAutoLogin/docs/README.md",
    "definitions": {
        "UsersDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Id": {
                    "type": "string"
                },
                "Password": {
                    "type": "string"
                },
                "Username": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AccessibilityErrorRedirectUrl": {
            "type": "string",
            "description": "Custom error page URL."
        },
        "AccessibilitySelfService": {
            "type": "boolean",
            "description": "Enable self-service. By default, it is `false`."
        },
        "AutoSubmitToolbar": {
            "type": "boolean",
            "description": "Display auto submit toolbar."
        },
        "CredentialsScheme": {
            "type": "string",
            "description": "One of: `\"EDIT_USERNAME_AND_PASSWORD\"`, `\"ADMIN_SETS_CREDENTIALS\"`, `\"EDIT_PASSWORD_ONLY\"`, `\"EXTERNAL_PASSWORD_SYNC\"`, or `\"SHARED_USERNAME_AND_PASSWORD\"`."
        },
        "Groups": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "HideIos": {
            "type": "boolean",
            "description": "Do not display application icon on mobile app."
        },
        "HideWeb": {
            "type": "boolean",
            "description": "Do not display application icon to users."
        },
        "Id": {
            "type": "string"
        },
        "Label": {
            "type": "string",
            "description": "The Application's display name."
        },
        "Name": {
            "type": "string"
        },
        "PreconfiguredApp": {
            "type": "string",
            "description": "Tells Okta to use an existing application in their application catalog, as opposed to a custom application."
        },
        "RevealPassword": {
            "type": "boolean"
        },
        "SharedPassword": {
            "type": "string",
            "description": "Shared password, required for certain schemes."
        },
        "SharedUsername": {
            "type": "string",
            "description": "Shared username, required for certain schemes."
        },
        "SignOnMode": {
            "type": "string"
        },
        "SignOnRedirectUrl": {
            "type": "string",
            "description": "Redirect URL; if going to the login page URL redirects to another page, then enter that URL here."
        },
        "SignOnUrl": {
            "type": "string"
        },
        "Status": {
            "type": "string",
            "description": "The status of the application, by default, it is `\"ACTIVE\"`."
        },
        "UserNameTemplate": {
            "type": "string",
            "description": "Username template. Default: `\"${source.login}\"`."
        },
        "UserNameTemplateSuffix": {
            "type": "string",
            "description": "Username template suffix."
        },
        "UserNameTemplateType": {
            "type": "string",
            "description": "Username template type. Default: `\"BUILT_IN\"`."
        },
        "Users": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/UsersDefinition"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "Label"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id",
        "/properties/Name",
        "/properties/SignOnMode"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    }
}