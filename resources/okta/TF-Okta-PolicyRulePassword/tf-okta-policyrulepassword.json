{
    "typeName": "TF::Okta::PolicyRulePassword",
    "description": "Creates a Password Policy Rule.\n\nThis resource allows you to create and configure a Password Policy Rule.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/okta/TF-Okta-PolicyRulePassword/docs/README.md",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "Policy Rule Name."
        },
        "NetworkConnection": {
            "type": "string",
            "description": "Network selection mode: `\"ANYWHERE\"`, `\"ZONE\"`, `\"ON_NETWORK\"`, or `\"OFF_NETWORK\"`."
        },
        "NetworkExcludes": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "The network zones to exclude. Conflicts with `network_includes`."
        },
        "NetworkIncludes": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "The network zones to include. Conflicts with `network_excludes`."
        },
        "PasswordChange": {
            "type": "string",
            "description": "Allow or deny a user to change their password: `\"ALLOW\"` or `\"DENY\"`. By default, it is `\"ALLOW\"`."
        },
        "PasswordReset": {
            "type": "string",
            "description": "Allow or deny a user to reset their password: `\"ALLOW\"` or `\"DENY\"`. By default, it is `\"ALLOW\"`."
        },
        "PasswordUnlock": {
            "type": "string",
            "description": "Allow or deny a user to unlock: `\"ALLOW\"` or `\"DENY\"`. By default, it is `\"DENY\"`,."
        },
        "Policyid": {
            "type": "string",
            "description": "Policy ID."
        },
        "Priority": {
            "type": "number",
            "description": "Policy Rule Priority, this attribute can be set to a valid priority. To avoid endless diff situation we error if an invalid priority is provided. API defaults it to the last (lowest) if not there."
        },
        "Status": {
            "type": "string",
            "description": "Policy Rule Status: `\"ACTIVE\"` or `\"INACTIVE\"`."
        },
        "UsersExcluded": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "Policyid"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    }
}