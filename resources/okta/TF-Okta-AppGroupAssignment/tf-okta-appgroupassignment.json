        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AppId": {
            "type": "string",
            "description": "The ID of the application to assign a group to."
        },
        "GroupId": {
            "type": "string",
            "description": "The ID of the group to assign the app to."
        },
        "Id": {
            "type": "string"
        },
        "Priority": {
            "type": "number"
        },
        "Profile": {
