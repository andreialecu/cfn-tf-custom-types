        },
        "CanPasswordBeEdited": {
            "type": "boolean"
        },
        "DisplayName": {
            "type": "string"
        },
        "EmailAddress": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "IsActive": {
            "type": "boolean"
        },
        "IsRequestor": {
            "type": "boolean"
        },
        "IsService": {
            "type": "boolean"
        },
        "Password": {
            "type": "string"
        },
        "Username": {
            "type": "string"
        },
        "Identity": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/IdentityDefinition"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "DisplayName",
        "Username"
    ],
    "readOnlyProperties": [
