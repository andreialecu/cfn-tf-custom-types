    "definitions": {
        "IdentityDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Provider": {
                    "type": "string"
                },
                "Claim": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "$ref": "#/definitions/ClaimDefinition"
                    }
                }
            },
            "required": []
        },
        "ClaimDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "IsIdentifyingClaim": {
                    "type": "boolean"
                },
                "Name": {
                    "type": "string"
                },
                "Value": {
                    "type": "string"
                }
            },
            "required": [
                "IsIdentifyingClaim",
                "Name",
                "Value"
            ]
        }
    },
    "properties": {
