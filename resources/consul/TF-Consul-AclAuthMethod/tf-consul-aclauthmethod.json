{
    "typeName": "TF::Consul::AclAuthMethod",
    "description": "Starting with Consul 1.5.0, the consul_acl_auth_method resource can be used to\nmanaged [Consul ACL auth methods](https://www.consul.io/docs/acl/auth-methods).",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/consul/TF-Consul-AclAuthMethod/docs/README.md",
    "definitions": {
        "ConfigDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "MapKey": {
                    "type": "string"
                },
                "MapValue": {
                    "type": "string"
                }
            },
            "required": [
                "MapKey",
                "MapValue"
            ]
        },
        "NamespaceRuleDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "BindNamespace": {
                    "type": "string"
                },
                "Selector": {
                    "type": "string"
                }
            },
            "required": [
                "BindNamespace"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Config": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/ConfigDefinition"
            },
            "description": "The raw configuration for this ACL auth method. This\nattribute is deprecated and will be removed in a future version. `config_json`\nshould be used instead."
        },
        "ConfigJson": {
            "type": "string",
            "description": "The raw configuration for this ACL auth method."
        },
        "Description": {
            "type": "string",
            "description": "A free form human readable description of the auth method."
        },
        "DisplayName": {
            "type": "string",
            "description": "An optional name to use instead of the name\nattribute when displaying information about this auth method."
        },
        "Id": {
            "type": "string"
        },
        "MaxTokenTtl": {
            "type": "string",
            "description": "The maximum life of any token created by this\nauth method."
        },
        "Name": {
            "type": "string",
            "description": "The name of the ACL auth method."
        },
        "Namespace": {
            "type": "string",
            "description": "The namespace to create the policy within."
        },
        "TokenLocality": {
            "type": "string",
            "description": "The kind of token that this auth method\nproduces. This can be either 'local' or 'global'."
        },
        "Type": {
            "type": "string",
            "description": "The type of the ACL auth method."
        },
        "NamespaceRule": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/NamespaceRuleDefinition"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "Type"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    }
}