{
    "typeName": "TF::Quorum::BootstrapKeystore",
    "description": "Use this resource to create a keystore which maintains multiple Ethereum accounts.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/quorum/TF-Quorum-BootstrapKeystore/docs/README.md",
    "definitions": {
        "AccountDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "AccountUrl": {
                    "type": "string"
                },
                "Address": {
                    "type": "string"
                },
                "Balance": {
                    "type": "string"
                },
                "Passphrase": {
                    "type": "string"
                }
            }
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Account": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/AccountDefinition"
            },
            "description": "Account being created under this keystore."
        },
        "Id": {
            "type": "string"
        },
        "KeystoreDir": {
            "type": "string",
            "description": "Directory contains private keys\n- `use_light_weight_kdf` - (Optional) True to lower the memory and CPU requirements of the key store scrypt KDF at the expense of security."
        },
        "KeystoreDirAbs": {
            "type": "string"
        },
        "UseLightWeightKdf": {
            "type": "boolean",
            "description": "True to lower the memory and CPU requirements of the key store scrypt KDF at the expense of security."
        }
    },
    "additionalProperties": false,
    "required": [
        "KeystoreDir"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id",
        "/properties/KeystoreDirAbs"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    }
}