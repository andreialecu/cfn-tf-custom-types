        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "Name of a new network. Directory name restriction applied\n- `target_dir` - (Optional) File system path to the directory on which new directory will be created. Default is current working directory."
