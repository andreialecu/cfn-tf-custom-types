        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Description": {
            "type": "string",
            "description": "Description for this zone."
        },
        "Email": {
            "type": "string",
            "description": "E-mail for the zone.\nUsed in SOA records for the zone.\nThis parameter is not currently supported.\nEven if you set this parameter, it will be ignored."
        },
        "Id": {
            "type": "string"
        },
        "Masters": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "For secondary zones.\nThe servers to slave from to get DNS information."
        },
        "Name": {
            "type": "string",
            "description": "DNS Name for the zone."
        },
        "Ttl": {
            "type": "number",
            "description": "TTL (Time to Live) for the zone.\nThis parameter is not currently supported.\nEven if you set this parameter, it will be ignored."
        },
        "Type": {
            "type": "string",
            "description": "Type of zone.\nPRIMARY is controlled by ECL2.0 DNS,\nSECONDARY zones are slaved from another DNS Server.\nDefaults to PRIMARY.\nThis parameter is not currently supported.\nEven if you set this parameter, it will be ignored."
        },
        "Timeouts": {
            "$ref": "#/definitions/TimeoutsDefinition"
        }
    },
    "additionalProperties": false,
    "required": [
