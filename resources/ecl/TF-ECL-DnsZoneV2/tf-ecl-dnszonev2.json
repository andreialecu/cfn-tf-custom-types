    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/ecl/TF-ECL-DnsZoneV2/docs/README.md",
    "definitions": {
        "TimeoutsDefinition": {
            "type": "object",
