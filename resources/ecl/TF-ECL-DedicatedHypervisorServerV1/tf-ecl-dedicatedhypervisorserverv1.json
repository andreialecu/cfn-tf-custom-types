{
    "typeName": "TF::ECL::DedicatedHypervisorServerV1",
    "description": "Manages a dedicated hypervisor v1 Server resource within Enterprise Cloud.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/ecl/TF-ECL-DedicatedHypervisorServerV1/docs/README.md",
    "definitions": {
        "MetadataDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "MapKey": {
                    "type": "string"
                },
                "MapValue": {
                    "type": "string"
                }
            },
            "required": [
                "MapKey",
                "MapValue"
            ]
        },
        "NetworksDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "FixedIp": {
                    "type": "string",
                    "description": "Specifies a fixed IPv4 address to be used on this network.\nChanging this creates a new server."
                },
                "Plane": {
                    "type": "string",
                    "description": "The traffic type of a network to attach to the server. `data` and `storage` are supported.\nChanging this creates a new server."
                },
                "Port": {
                    "type": "string",
                    "description": "The port UUID of a network to attach to the server.\nChanging this creates a new server."
                },
                "SegmentationId": {
                    "type": "number",
                    "description": "The segmentation ID of a network to attach to the server.\nThis value is integer, no less than 4 and no more than 4093. Changing this creates a new server."
                },
                "Uuid": {
                    "type": "string",
                    "description": "The network UUID to attach to the server.\nChanging this creates a new server."
                }
            },
            "required": [
                "Plane",
                "SegmentationId"
            ]
        },
        "TimeoutsDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AdminPass": {
            "type": "string",
            "description": "Password for the administrator. Changing this creates a new server."
        },
        "AvailabilityZone": {
            "type": "string",
            "description": "The availability zone name in which to launch the server.\nIf omit this parameter, target availability_zone is random. Changing this creates a new server."
        },
        "BaremetalServerId": {
            "type": "string"
        },
        "Description": {
            "type": "string",
            "description": "Description of your Dedicated Hypervisor server as a string.\nChanging this creates a new server."
        },
        "FlavorRef": {
            "type": "string",
            "description": "The flavor reference for the desired flavor for your Dedicated Hypervisor server.\nSpecify as an UUID or full URL. Changing this creates a new server."
        },
        "Id": {
            "type": "string"
        },
        "ImageRef": {
            "type": "string",
            "description": "The image reference for the desired image for your Dedicated Hypervisor server.\nSpecify as an UUID or full URL. Changing this creates a new server."
        },
        "Metadata": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/MetadataDefinition"
            },
            "description": "Metadata key and value pairs. Changing this creates a new server."
        },
        "Name": {
            "type": "string",
            "description": "Name of your Dedicated Hypervisor/Baremetal server as a string."
        },
        "Networks": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/NetworksDefinition"
            },
            "maxItems": 4,
            "minItems": 1
        },
        "Timeouts": {
            "$ref": "#/definitions/TimeoutsDefinition"
        }
    },
    "additionalProperties": false,
    "required": [
        "FlavorRef",
        "ImageRef",
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/BaremetalServerId",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    }
}