{
    "typeName": "TF::ECL::SecurityNetworkBasedDeviceHaV2",
    "description": "Manages a V2 Network Based Device(Single) resource within Enterprise Cloud.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/ecl/TF-ECL-SecurityNetworkBasedDeviceHaV2/docs/README.md",
    "definitions": {
        "HaLink1Definition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Host1IpAddress": {
                    "type": "string"
                },
                "Host2IpAddress": {
                    "type": "string"
                },
                "NetworkId": {
                    "type": "string"
                },
                "SubnetId": {
                    "type": "string"
                }
            },
            "required": [
                "Host1IpAddress",
                "Host2IpAddress",
                "NetworkId",
                "SubnetId"
            ]
        },
        "HaLink2Definition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Host1IpAddress": {
                    "type": "string",
                    "description": "Set IPv4 address for as HA Host 1 in this HA link."
                },
                "Host2IpAddress": {
                    "type": "string",
                    "description": "Set IPv4 address for as HA Host 2 in this HA link."
                },
                "NetworkId": {
                    "type": "string",
                    "description": "Set the Network ID to be used for HA line."
                },
                "SubnetId": {
                    "type": "string",
                    "description": "Set the Subnet ID to be used for HA line."
                }
            },
            "required": [
                "Host1IpAddress",
                "Host2IpAddress",
                "NetworkId",
                "SubnetId"
            ]
        },
        "PortDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Comment": {
                    "type": "string",
                    "description": "Comments for the port."
                },
                "Enable": {
                    "type": "string",
                    "description": "Set \"true\" to enable the port \"false\" to disable the port."
                },
                "EnablePing": {
                    "type": "string",
                    "description": "Set \"true\" to enable ping response, \"false\" to disable ping response."
                },
                "Host1IpAddress": {
                    "type": "string",
                    "description": "IP Address of the port for HA Host 1."
                },
                "Host1IpAddressPrefix": {
                    "type": "number",
                    "description": "IP Address prefix of the port for HA Host 1."
                },
                "Host2IpAddress": {
                    "type": "string",
                    "description": "IP Address of the port for HA Host 2."
                },
                "Host2IpAddressPrefix": {
                    "type": "number",
                    "description": "IP Address prefix of the port for HA Host 2."
                },
                "Mtu": {
                    "type": "string",
                    "description": "MTU value in the configuration of the port."
                },
                "NetworkId": {
                    "type": "string",
                    "description": "Network ID to which the port is associated."
                },
                "Preempt": {
                    "type": "string",
                    "description": "\"true\" to enable preempt option, \"false\" to disable preempt option."
                },
                "SubnetId": {
                    "type": "string",
                    "description": "UUID\tSubnet IDto which the port is associated."
                },
                "VrrpGrpId": {
                    "type": "string",
                    "description": "VRRP Group ID. This value must be in the range of \"1\" to \"100\"."
                },
                "VrrpId": {
                    "type": "string",
                    "description": "VRRP ID. This value must be in the range of \"1\" to \"100\"."
                },
                "VrrpIpAddress": {
                    "type": "string",
                    "description": "VRRP IP Address of this port."
                }
            },
            "required": [
                "Enable"
            ]
        },
        "TimeoutsDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Host1AzGroup": {
            "type": "string",
            "description": "Set availability zone for HA Host 1."
        },
        "Host2AzGroup": {
            "type": "string",
            "description": "Set availability zone for HA Host 2."
        },
        "Id": {
            "type": "string"
        },
        "LicenseKind": {
            "type": "string",
            "description": "Set \"02\" or \"08\" as FW/UTM plan."
        },
        "Locale": {
            "type": "string",
            "description": "Messages are displayed in Japanese or English depending on this value.\nja: Japanese, en: English. Default value is \"en\"."
        },
        "OperatingMode": {
            "type": "string",
            "description": "Set \"FW\" or \"UTM\" to this value."
        },
        "TenantId": {
            "type": "string",
            "description": "Tenant ID of the owner (UUID)."
        },
        "HaLink1": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/HaLink1Definition"
            },
            "maxItems": 1,
            "minItems": 1
        },
        "HaLink2": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/HaLink2Definition"
            },
            "maxItems": 1,
            "minItems": 1
        },
        "Port": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/PortDefinition"
            },
            "maxItems": 7
        },
        "Timeouts": {
            "$ref": "#/definitions/TimeoutsDefinition"
        }
    },
    "additionalProperties": false,
    "required": [
        "Host1AzGroup",
        "Host2AzGroup",
        "LicenseKind",
        "OperatingMode",
        "TenantId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    }
}