                "Nexthop": {
                    "type": "string",
                    "description": "Nexthop IP address of this routing."
                }
            },
            "required": [
                "Destination",
                "Nexthop"
            ]
        },
        "TimeoutsDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
