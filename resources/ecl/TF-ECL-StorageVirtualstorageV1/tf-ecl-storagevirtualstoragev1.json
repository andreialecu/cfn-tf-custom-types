            "properties": {
                "MapKey": {
                    "type": "string"
                },
                "MapValue": {
                    "type": "string"
                }
            },
            "required": [
                "MapKey",
                "MapValue"
            ]
        },
        "HostRoutesDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Destination": {
                    "type": "string",
