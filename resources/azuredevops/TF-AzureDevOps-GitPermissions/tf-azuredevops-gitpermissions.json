{
    "typeName": "TF::AzureDevOps::GitPermissions",
    "description": "Manages permissions for Git repositories. \n\n~> **Note** Permissions can be assigned to group principals and not to single user principals.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/azuredevops/TF-AzureDevOps-GitPermissions/docs/README.md",
    "definitions": {
        "PermissionsDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "MapKey": {
                    "type": "string"
                },
                "MapValue": {
                    "type": "string"
                }
            },
            "required": [
                "MapKey",
                "MapValue"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "BranchName": {
            "type": "string",
            "description": "The name of the branch to assign the permissions."
        },
        "Id": {
            "type": "string"
        },
        "Permissions": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/PermissionsDefinition"
            },
            "description": "the permissions to assign. The follwing permissions are available."
        },
        "Principal": {
            "type": "string",
            "description": "The **group** principal to assign the permissions."
        },
        "ProjectId": {
            "type": "string",
            "description": "The ID of the project to assign the permissions."
        },
        "Replace": {
            "type": "boolean",
            "description": "Replace (`true`) or merge (`false`) the permissions. Default: `true`."
        },
        "RepositoryId": {
            "type": "string",
            "description": "The ID of the GIT repository to assign the permissions."
        }
    },
    "additionalProperties": false,
    "required": [
        "Permissions",
        "Principal",
        "ProjectId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    }
}