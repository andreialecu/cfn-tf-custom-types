{
    "typeName": "TF::FortiOS::SystemMobiletunnel",
    "description": "Configure Mobile tunnels, an implementation of Network Mobility (NEMO) extensions for Mobile IPv4 RFC5177.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/fortios/TF-FortiOS-SystemMobiletunnel/docs/README.md",
    "definitions": {
        "NetworkDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Id": {
                    "type": "number",
                    "description": "Network entry ID."
                },
                "Interface": {
                    "type": "string",
                    "description": "Select the associated interface name from available options."
                },
                "Prefix": {
                    "type": "string",
                    "description": "Class IP and Netmask with correction (Format:xxx.xxx.xxx.xxx xxx.xxx.xxx.xxx or xxx.xxx.xxx.xxx/x)."
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "DynamicSortSubtable": {
            "type": "string",
            "description": "true or false, set this parameter to true when using dynamic for_each + toset to configure and sort sub-tables, please do not set this parameter when configuring static sub-tables."
        },
        "HashAlgorithm": {
            "type": "string",
            "description": "Hash Algorithm (Keyed MD5). Valid values: `hmac-md5`."
        },
        "HomeAddress": {
            "type": "string",
            "description": "Home IP address (Format: xxx.xxx.xxx.xxx)."
        },
        "HomeAgent": {
            "type": "string",
            "description": "IPv4 address of the NEMO HA (Format: xxx.xxx.xxx.xxx)."
        },
        "Id": {
            "type": "string"
        },
        "Lifetime": {
            "type": "number",
            "description": "NMMO HA registration request lifetime (180 - 65535 sec, default = 65535)."
        },
        "NMhaeKey": {
            "type": "string",
            "description": "NEMO authentication key."
        },
        "NMhaeKeyType": {
            "type": "string",
            "description": "NEMO authentication key type (ascii or base64). Valid values: `ascii`, `base64`."
        },
        "NMhaeSpi": {
            "type": "number",
            "description": "NEMO authentication SPI (default: 256)."
        },
        "Name": {
            "type": "string",
            "description": "Tunnel name."
        },
        "RegInterval": {
            "type": "number",
            "description": "NMMO HA registration interval (5 - 300, default = 5)."
        },
        "RegRetry": {
            "type": "number",
            "description": "Maximum number of NMMO HA registration retries (1 to 30, default = 3)."
        },
        "RenewInterval": {
            "type": "number",
            "description": "Time before lifetime expiraton to send NMMO HA re-registration (5 - 60, default = 60)."
        },
        "RoamingInterface": {
            "type": "string",
            "description": "Select the associated interface name from available options."
        },
        "Status": {
            "type": "string",
            "description": "Enable/disable this mobile tunnel. Valid values: `disable`, `enable`."
        },
        "TunnelMode": {
            "type": "string",
            "description": "NEMO tunnnel mode (GRE tunnel). Valid values: `gre`."
        },
        "Vdomparam": {
            "type": "string",
            "description": "Specifies the vdom to which the resource will be applied when the FortiGate unit is running in VDOM mode. Only one vdom can be specified. If you want to inherit the vdom configuration of the provider, please do not set this parameter."
        },
        "Network": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/NetworkDefinition"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "HashAlgorithm",
        "HomeAgent",
        "Lifetime",
        "NMhaeKeyType",
        "NMhaeSpi",
        "RegInterval",
        "RegRetry",
        "RenewInterval",
        "RoamingInterface",
        "TunnelMode"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    },
    "writeOnlyProperties": [
        "/properties/NMhaeKey"
    ]
}