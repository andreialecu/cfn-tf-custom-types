        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "Name."
        },
        "Range": {
            "type": "string",
            "description": "Either the global or VDOM IP address range for the remote certificate. Valid values: `global`, `vdom`."
        },
        "Remote": {
            "type": "string",
            "description": "Remote certificate."
        },
        "Source": {
            "type": "string",
