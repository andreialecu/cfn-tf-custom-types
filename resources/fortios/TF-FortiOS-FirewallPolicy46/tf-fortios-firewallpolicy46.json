        },
        "Fixedport": {
            "type": "string",
            "description": "Enable/disable fixed port for this policy. Valid values: `enable`, `disable`."
        },
        "Id": {
            "type": "string"
        },
        "Ippool": {
            "type": "string",
            "description": "Enable/disable use of IP Pools for source NAT. Valid values: `enable`, `disable`."
