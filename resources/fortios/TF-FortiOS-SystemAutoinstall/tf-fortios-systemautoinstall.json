    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AutoInstallConfig": {
            "type": "string",
            "description": "Enable/disable auto install the config in USB disk. Valid values: `enable`, `disable`."
        },
        "AutoInstallImage": {
            "type": "string",
            "description": "Enable/disable auto install the image in USB disk. Valid values: `enable`, `disable`."
        },
        "DefaultConfigFile": {
            "type": "string",
            "description": "Default config file name in USB disk."
        },
        "DefaultImageFile": {
            "type": "string",
            "description": "Default image file name in USB disk."
        },
        "Id": {
            "type": "string"
        },
        "Vdomparam": {
            "type": "string",
            "description": "Specifies the vdom to which the resource will be applied when the FortiGate unit is running in VDOM mode. Only one vdom can be specified. If you want to inherit the vdom configuration of the provider, please do not set this parameter."
        }
    },
    "additionalProperties": false,
