{
    "typeName": "TF::FortiOS::DlpFilepattern",
    "description": "Configure file patterns used by DLP blocking.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/fortios/TF-FortiOS-DlpFilepattern/docs/README.md",
    "definitions": {
        "EntriesDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "FileType": {
                    "type": "string",
                    "description": "Select a file type."
                },
                "FilterType": {
                    "type": "string",
                    "description": "Filter by file name pattern or by file type. Valid values: `pattern`, `type`."
                },
                "Pattern": {
                    "type": "string",
                    "description": "Add a file name pattern."
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Comment": {
            "type": "string",
            "description": "Optional comments."
        },
        "DynamicSortSubtable": {
            "type": "string",
            "description": "true or false, set this parameter to true when using dynamic for_each + toset to configure and sort sub-tables, please do not set this parameter when configuring static sub-tables."
        },
        "Fosid": {
            "type": "number",
            "description": "ID."
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "Name of table containing the file pattern list."
        },
        "Vdomparam": {
            "type": "string",
            "description": "Specifies the vdom to which the resource will be applied when the FortiGate unit is running in VDOM mode. Only one vdom can be specified. If you want to inherit the vdom configuration of the provider, please do not set this parameter."
        },
        "Entries": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/EntriesDefinition"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "Fosid",
        "Name"
    ],
    "readOnlyProperties": [
