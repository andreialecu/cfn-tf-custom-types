            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Action": {
                    "type": "string",
                    "description": "Permit or deny this IP address and netmask prefix. Valid values: `permit`, `deny`."
                },
                "ExactMatch": {
                    "type": "string",
                    "description": "Enable/disable exact prefix match. Valid values: `enable`, `disable`."
                },
                "Flags": {
                    "type": "number",
                    "description": "Flags."
                },
                "Id": {
                    "type": "number",
                    "description": "Rule ID."
                },
                "Prefix6": {
                    "type": "string",
                    "description": "IPv6 prefix to define regular filter criteria, such as \"any\" or subnets."
                }
            },
            "required": []
        }
    },
    "properties": {
