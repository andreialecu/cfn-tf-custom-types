{
    "typeName": "TF::FortiOS::FmgDevicemanagerScriptExecute",
    "description": "This resource supports executing devicemanager script on Fortimanager.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/fortios/TF-FortiOS-FmgDevicemanagerScriptExecute/docs/README.md",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Adom": {
            "type": "string",
            "description": "Source ADOM name. default is 'root'."
        },
        "Id": {
            "type": "string"
        },
        "Package": {
            "type": "string",
            "description": "Policy package."
        },
        "ScriptName": {
            "type": "string",
            "description": "Script name."
        },
        "TargetDevname": {
            "type": "string",
            "description": "Target device name, which the script will be installed."
        },
        "Timeout": {
            "type": "number",
            "description": "Timeout(minute) for executing the script, default is 3 minutes."
        },
        "Vdom": {
            "type": "string",
            "description": "Vdom of managed device. default is 'root'."
        }
    },
    "additionalProperties": false,
    "required": [
        "ScriptName"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    }
}