                "SwitchBackTime": {
                    "type": "string",
                    "description": "Automatically switch over to preferred SIM/carrier at a specified time in UTC (HH:MM)."
                },
                "SwitchBackTimer": {
                    "type": "number",
                    "description": "Automatically switch over to preferred SIM/carrier after the given time (3600 - 2147483647 sec)."
                }
            },
            "required": []
        }
