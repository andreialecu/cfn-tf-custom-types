{
    "typeName": "TF::FortiOS::IpsCustom",
    "description": "Configure IPS custom signature.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/fortios/TF-FortiOS-IpsCustom/docs/README.md",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Action": {
            "type": "string",
            "description": "Default action (pass or block) for this signature. Valid values: `pass`, `block`."
        },
        "Application": {
            "type": "string",
            "description": "Applications to be protected. Blank for all applications."
        },
        "Comment": {
            "type": "string",
            "description": "Comment."
        },
        "Id": {
            "type": "string"
        },
        "Location": {
            "type": "string",
            "description": "Protect client or server traffic."
        },
        "Log": {
            "type": "string",
            "description": "Enable/disable logging. Valid values: `disable`, `enable`."
        },
        "LogPacket": {
            "type": "string",
            "description": "Enable/disable packet logging. Valid values: `disable`, `enable`."
        },
        "Os": {
            "type": "string",
            "description": "Operating system(s) that the signature protects. Blank for all operating systems."
        },
        "Protocol": {
            "type": "string",
            "description": "Protocol(s) that the signature scans. Blank for all protocols."
        },
        "RuleId": {
            "type": "number",
            "description": "Signature ID."
        },
        "Severity": {
            "type": "string",
            "description": "Relative severity of the signature, from info to critical. Log messages generated by the signature include the severity."
        },
        "SigName": {
            "type": "string",
            "description": "Signature name."
        },
        "Signature": {
            "type": "string",
            "description": "Custom signature enclosed in single quotes."
        },
        "Status": {
            "type": "string",
            "description": "Enable/disable this signature. Valid values: `disable`, `enable`."
        },
        "Tag": {
            "type": "string",
            "description": "Signature tag."
        },
        "Vdomparam": {
            "type": "string",
            "description": "Specifies the vdom to which the resource will be applied when the FortiGate unit is running in VDOM mode. Only one vdom can be specified. If you want to inherit the vdom configuration of the provider, please do not set this parameter."
        }
    },
    "additionalProperties": false,
    "required": [],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    }
}