            },
            "required": [
                "Active",
                "Unscanned"
            ]
        },
        "CvssRangeDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "From": {
                    "type": "number"
                },
                "To": {
                    "type": "number"
                }
            },
            "required": [
                "From",
                "To"
            ]
        }
