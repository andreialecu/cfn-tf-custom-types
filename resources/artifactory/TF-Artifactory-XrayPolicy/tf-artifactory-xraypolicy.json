        "CriteriaDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "AllowUnknown": {
                    "type": "boolean"
                },
                "AllowedLicenses": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "type": "string"
                    }
                },
                "BannedLicenses": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "type": "string"
                    }
                },
                "MinSeverity": {
                    "type": "string"
                },
                "CvssRange": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/CvssRangeDefinition"
