                "FailBuild": {
                    "type": "boolean"
                },
                "Mails": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "type": "string"
                    }
                },
                "Webhooks": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "type": "string"
                    }
                },
                "BlockDownload": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/BlockDownloadDefinition"
                    },
                    "maxItems": 1,
                    "minItems": 1
                }
            },
            "required": []
