{
    "typeName": "TF::AzureRM::DnsCaaRecord",
    "description": "Enables you to manage DNS CAA Records within Azure DNS.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/azurerm/TF-AzureRM-DnsCaaRecord/docs/README.md",
    "definitions": {
        "TagsDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "MapKey": {
                    "type": "string"
                },
                "MapValue": {
                    "type": "string"
                }
            },
            "required": [
                "MapKey",
                "MapValue"
            ]
        },
        "RecordDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Flags": {
                    "type": "number",
                    "description": "Extensible CAA flags, currently only 1 is implemented to set the issuer critical flag."
                },
                "Tag": {
                    "type": "string",
                    "description": "A property tag, options are issue, issuewild and iodef."
                },
                "Value": {
                    "type": "string",
                    "description": "A property value such as a registrar domain."
                }
            },
            "required": [
                "Flags",
                "Tag",
                "Value"
            ]
        },
        "TimeoutsDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Read": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Fqdn": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The name of the DNS CAA Record. If you are creating the record in the apex of the zone use `\"@\"` as the name."
        },
        "ResourceGroupName": {
            "type": "string",
            "description": "Specifies the resource group where the DNS Zone (parent resource) exists. Changing this forces a new resource to be created."
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/TagsDefinition"
            },
            "description": "A mapping of tags to assign to the resource."
        },
        "Ttl": {
            "type": "number",
            "description": "The Time To Live (TTL) of the DNS record in seconds."
        },
        "ZoneName": {
            "type": "string",
            "description": "Specifies the DNS Zone where the resource exists. Changing this forces a new resource to be created."
        },
        "Record": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/RecordDefinition"
            },
            "minItems": 1
        },
        "Timeouts": {
            "$ref": "#/definitions/TimeoutsDefinition"
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "ResourceGroupName",
        "Ttl",
        "ZoneName"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Fqdn",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    }
}