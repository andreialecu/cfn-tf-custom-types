{
    "typeName": "TF::AzureRM::LogicAppTriggerRecurrence",
    "description": "Manages a Recurrence Trigger within a Logic App Workflow",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/azurerm/TF-AzureRM-LogicAppTriggerRecurrence/docs/README.md",
    "definitions": {
        "ScheduleDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "AtTheseHours": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "number"
                    },
                    "description": "Specifies a list of hours when the trigger should run. Valid values are between 0 and 23."
                },
                "AtTheseMinutes": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "number"
                    },
                    "description": "Specifies a list of minutes when the trigger should run. Valid values are between 0 and 59."
                },
                "OnTheseDays": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    },
                    "description": "Specifies a list of days when the trigger should run. Valid values include `Monday`, `Tuesday`, `Wednesday`, `Thursday`, `Friday`, `Saturday`, and `Sunday`."
                }
            },
            "required": []
        },
        "TimeoutsDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Read": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Frequency": {
            "type": "string",
            "description": "Specifies the Frequency at which this Trigger should be run. Possible values include `Month`, `Week`, `Day`, `Hour`, `Minute` and `Second`."
        },
        "Id": {
            "type": "string"
        },
        "Interval": {
            "type": "number",
            "description": "Specifies interval used for the Frequency, for example a value of `4` for `interval` and `hour` for `frequency` would run the Trigger every 4 hours."
        },
        "LogicAppId": {
            "type": "string",
            "description": "Specifies the ID of the Logic App Workflow. Changing this forces a new resource to be created."
        },
        "Name": {
            "type": "string",
            "description": "Specifies the name of the Recurrence Triggers to be created within the Logic App Workflow. Changing this forces a new resource to be created."
        },
        "StartTime": {
            "type": "string",
            "description": "Specifies the start date and time for this trigger in RFC3339 format: `2000-01-02T03:04:05Z`."
        },
        "TimeZone": {
            "type": "string",
            "description": "Specifies the time zone for this trigger.  Supported time zone options are listed [here](https://support.microsoft.com/en-us/help/973627/microsoft-time-zone-index-values)."
        },
        "Schedule": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/ScheduleDefinition"
            },
            "maxItems": 1
        },
        "Timeouts": {
            "$ref": "#/definitions/TimeoutsDefinition"
        }
    },
    "additionalProperties": false,
    "required": [
        "Frequency",
        "Interval",
        "LogicAppId",
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    }
}