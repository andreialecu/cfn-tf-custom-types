{
    "typeName": "TF::AzureRM::SpringCloudApp",
    "description": "Manage an Azure Spring Cloud Application.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/azurerm/TF-AzureRM-SpringCloudApp/docs/README.md",
    "definitions": {
        "IdentityDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Type": {
                    "type": "string",
                    "description": "Specifies the identity type of the Spring Cloud Application. Possible value is `SystemAssigned`."
                }
            },
            "required": []
        },
        "PersistentDiskDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "MountPath": {
                    "type": "string",
                    "description": "Specifies the mount path of the persistent disk. Defaults to `/persistent`."
                },
                "SizeInGb": {
                    "type": "number",
                    "description": "Specifies the size of the persistent disk in GB. Possible values are between `0` and `50`."
                }
            },
            "required": [
                "SizeInGb"
            ]
        },
        "TimeoutsDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Read": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Fqdn": {
            "type": "string"
        },
        "HttpsOnly": {
            "type": "boolean",
            "description": "Is only https allowed? Defaults to `false`."
        },
        "Id": {
            "type": "string"
        },
        "IsPublic": {
            "type": "boolean",
            "description": "Does the Spring Cloud Application have public endpoint? Defaults to `false`."
        },
        "Name": {
            "type": "string",
            "description": "Specifies the name of the Spring Cloud Application. Changing this forces a new resource to be created."
        },
        "ResourceGroupName": {
            "type": "string",
            "description": "Specifies the name of the resource group in which to create the Spring Cloud Application. Changing this forces a new resource to be created."
        },
        "ServiceName": {
            "type": "string",
            "description": "Specifies the name of the Spring Cloud Service resource. Changing this forces a new resource to be created."
        },
        "TlsEnabled": {
            "type": "boolean",
            "description": "Is End to End TLS Enabled? Defaults to `false`."
        },
        "Url": {
            "type": "string"
        },
        "Identity": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/IdentityDefinition"
            },
            "maxItems": 1
        },
        "PersistentDisk": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/PersistentDiskDefinition"
            },
            "maxItems": 1
        },
        "Timeouts": {
            "$ref": "#/definitions/TimeoutsDefinition"
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "ResourceGroupName",
        "ServiceName"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Fqdn",
        "/properties/Id",
        "/properties/Url"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    }
}