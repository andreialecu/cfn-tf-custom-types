        },
        "DatabaseName": {
            "type": "string",
            "description": "The name of the database which you would like to attach, use * if you want to follow all current and future databases."
        },
        "DefaultPrincipalModificationKind": {
            "type": "string",
            "description": "The default principals modification kind. Valid values are: `None` (default), `Replace` and `Union`."
        },
        "Id": {
            "type": "string"
        },
        "Location": {
            "type": "string",
