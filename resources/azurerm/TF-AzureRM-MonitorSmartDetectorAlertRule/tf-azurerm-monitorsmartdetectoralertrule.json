            "description": "Specifies the name of the resource group in which the Monitor Smart Detector Alert Rule should exist. Changing this forces a new resource to be created."
        },
        "ScopeResourceIds": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "Specifies the scopes of this Smart Detector Alert Rule."
        },
        "Severity": {
            "type": "string",
            "description": "Specifies the severity of this Smart Detector Alert Rule. Possible values are `Sev0`, `Sev1`, `Sev2`, `Sev3` or `Sev4`."
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/TagsDefinition"
            },
            "description": "A mapping of tags to assign to the resource."
        },
        "ThrottlingDuration": {
            "type": "string",
            "description": "Specifies the duration (in ISO8601 format) to wait before notifying on the alert rule again."
        },
        "ActionGroup": {
            "type": "array",
            "insertionOrder": false,
            "items": {
