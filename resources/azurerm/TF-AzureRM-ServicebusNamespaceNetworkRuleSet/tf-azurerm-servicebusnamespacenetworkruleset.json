    "definitions": {
        "NetworkRulesDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "IgnoreMissingVnetServiceEndpoint": {
                    "type": "boolean",
                    "description": "Should the ServiceBus Namespace Network Rule Set ignore missing Virtual Network Service Endpoint option in the Subnet? Defaults to `false`."
                },
                "SubnetId": {
                    "type": "string",
                    "description": "The Subnet ID which should be able to access this ServiceBus Namespace."
                }
            },
            "required": [
