                "Memory": {
                    "type": "number"
                },
                "RunningCpu": {
                    "type": "number"
                },
                "RunningMemory": {
                    "type": "number"
                },
                "RunningVms": {
                    "type": "number"
                },
                "SystemDiskSize": {
                    "type": "number"
                },
                "Vms": {
                    "type": "number"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Admins": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "number"
            },
            "description": "List of Administrator user IDs part of the group."
        },
        "DeleteOnDestruction": {
            "type": "boolean",
            "description": "Flag to delete the group on destruction. Defaults to `false`."
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The name of the group."
        },
        "Template": {
            "type": "string",
            "description": "Group template content in OpenNebula XML or String format. Used to provide SUSNTONE arguments."
        },
        "Users": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "number"
            }
        },
        "Quotas": {
            "type": "array",
            "insertionOrder": true,
            "items": {
