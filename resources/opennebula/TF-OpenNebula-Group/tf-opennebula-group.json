{
    "typeName": "TF::OpenNebula::Group",
    "description": "Provides an OpenNebula group resource.\n\nThis resource allows you to manage groups on your OpenNebula clusters. When applied,\na new group is created. When destroyed, it is removed.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/opennebula/TF-OpenNebula-Group/docs/README.md",
    "definitions": {
        "QuotasDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "DatastoreQuotas": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/DatastoreQuotasDefinition"
                    }
                },
                "ImageQuotas": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/ImageQuotasDefinition"
                    }
                },
                "NetworkQuotas": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/NetworkQuotasDefinition"
                    }
                },
                "VmQuotas": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "$ref": "#/definitions/VmQuotasDefinition"
                    }
                }
            },
            "required": []
        },
        "DatastoreQuotasDefinition": {
            "type": "object",
