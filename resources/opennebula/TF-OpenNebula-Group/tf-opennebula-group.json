            "properties": {
                "Id": {
                    "type": "number"
                },
                "Images": {
                    "type": "number"
                },
                "Size": {
                    "type": "number"
                }
            },
            "required": [
                "Id"
            ]
        },
        "ImageQuotasDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Id": {
                    "type": "number"
                },
                "RunningVms": {
                    "type": "number"
