            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Name": {
                    "type": "string"
                },
                "StateId": {
                    "type": "string"
                },
                "Url": {
                    "type": "string"
                }
            }
        },
        "EventDataDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Account": {
                    "type": "string"
                },
                "RegionApiIdentifier": {
                    "type": "string"
                },
                "RegionId": {
                    "type": "number"
                }
            }
        },
        "TimeRangeDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Absolute": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/AbsoluteDefinition"
                    },
                    "maxItems": 1
                },
                "Relative": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/RelativeDefinition"
                    },
                    "maxItems": 1
                },
                "ToNow": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/ToNowDefinition"
                    },
                    "maxItems": 1
                }
            },
            "required": []
        },
        "AbsoluteDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "End": {
                    "type": "number"
                },
                "Start": {
                    "type": "number"
                }
            },
            "required": [
                "End",
                "Start"
            ]
        },
        "RelativeDefinition": {
            "type": "object",
