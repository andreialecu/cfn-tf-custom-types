{
    "typeName": "TF::AWS::DatasyncLocationSmb",
    "description": "Manages a SMB Location within AWS DataSync.\n\n~> **NOTE:** The DataSync Agents must be available before creating this resource.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/aws/TF-AWS-DatasyncLocationSmb/docs/README.md",
    "definitions": {
        "TagsDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "MapKey": {
                    "type": "string"
                },
                "MapValue": {
                    "type": "string"
                }
            },
            "required": [
                "MapKey",
                "MapValue"
            ]
        },
        "TagsAllDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "MapKey": {
                    "type": "string"
                },
                "MapValue": {
                    "type": "string"
                }
            },
            "required": [
                "MapKey",
                "MapValue"
            ]
        },
        "MountOptionsDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Version": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AgentArns": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "A list of DataSync Agent ARNs with which this location will be associated."
        },
        "Arn": {
            "type": "string"
        },
        "Domain": {
            "type": "string",
            "description": "The name of the Windows domain the SMB server belongs to."
        },
        "Id": {
            "type": "string"
        },
        "Password": {
            "type": "string",
            "description": "The password of the user who can mount the share and has file permissions in the SMB."
        },
        "ServerHostname": {
            "type": "string",
            "description": "Specifies the IP address or DNS name of the SMB server. The DataSync Agent(s) use this to mount the SMB share."
        },
        "Subdirectory": {
            "type": "string",
            "description": "Subdirectory to perform actions as source or destination. Should be exported by the NFS server."
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/TagsDefinition"
            },
            "description": "Key-value pairs of resource tags to assign to the DataSync Location. If configured with a provider [`default_tags` configuration block](/docs/providers/aws/index.html#default_tags-configuration-block) present, tags with matching keys will overwrite those defined at the provider-level."
        },
        "TagsAll": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/TagsAllDefinition"
            }
        },
        "Uri": {
            "type": "string"
        },
        "User": {
            "type": "string",
            "description": "The user who can mount the share and has file and folder permissions in the SMB share."
        },
        "MountOptions": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/MountOptionsDefinition"
            },
            "maxItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "AgentArns",
        "Password",
        "ServerHostname",
        "Subdirectory",
        "User"
