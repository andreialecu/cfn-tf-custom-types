{
    "typeName": "TF::AWS::OpsworksApplication",
    "description": "Provides an OpsWorks application resource.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/aws/TF-AWS-OpsworksApplication/docs/README.md",
    "definitions": {
        "AppSourceDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Password": {
                    "type": "string",
                    "description": "Password to use when authenticating to the source. Terraform cannot perform drift detection of this configuration."
                },
                "Revision": {
                    "type": "string",
                    "description": "For sources that are version-aware, the revision to use."
                },
                "SshKey": {
                    "type": "string",
                    "description": "SSH key to use when authenticating to the source. Terraform cannot perform drift detection of this configuration."
                },
                "Type": {
                    "type": "string",
                    "description": "The type of source to use. For example, \"archive\"."
                },
                "Url": {
                    "type": "string",
                    "description": "The URL where the app resource can be found."
                },
                "Username": {
                    "type": "string",
                    "description": "Username to use when authenticating to the source."
                }
            },
            "required": [
                "Type"
            ]
        },
        "EnvironmentDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Key": {
                    "type": "string",
                    "description": "Variable name."
                },
                "Secure": {
                    "type": "boolean",
                    "description": "Set visibility of the variable value to `true` or `false`."
                },
                "Value": {
                    "type": "string",
                    "description": "Variable value."
                }
            },
            "required": [
                "Key",
                "Value"
            ]
        },
        "SslConfigurationDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Certificate": {
                    "type": "string",
                    "description": "The contents of the certificate's domain.crt file."
                },
                "Chain": {
                    "type": "string",
                    "description": "Can be used to specify an intermediate certificate authority key or client authentication."
                },
                "PrivateKey": {
                    "type": "string",
                    "description": "The private key; the contents of the certificate's domain.key file."
                }
            },
            "required": [
                "Certificate",
                "PrivateKey"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AutoBundleOnDeploy": {
            "type": "string",
            "description": "Run bundle install when deploying for application of type `rails`."
        },
        "AwsFlowRubySettings": {
            "type": "string",
            "description": "Specify activity and workflow workers for your app using the aws-flow gem."
        },
        "DataSourceArn": {
            "type": "string",
            "description": "The data source's ARN."
        },
        "DataSourceDatabaseName": {
            "type": "string",
            "description": "The database name."
        },
        "DataSourceType": {
            "type": "string",
            "description": "The data source's type one of `AutoSelectOpsworksMysqlInstance`, `OpsworksMysqlInstance`, or `RdsDbInstance`."
        },
        "Description": {
            "type": "string",
            "description": "A description of the app."
        },
        "DocumentRoot": {
            "type": "string",
            "description": "Subfolder for the document root for application of type `rails`."
        },
        "Domains": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "A list of virtual host alias."
        },
        "EnableSsl": {
            "type": "boolean",
            "description": "Whether to enable SSL for the app. This must be set in order to let `ssl_configuration.private_key`, `ssl_configuration.certificate` and `ssl_configuration.chain` take effect."
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "A human-readable name for the application."
        },
        "RailsEnv": {
            "type": "string",
            "description": "The name of the Rails environment for application of type `rails`."
        },
        "ShortName": {
            "type": "string",
            "description": "A short, machine-readable name for the application. This can only be defined on resource creation and ignored on resource update."
        },
        "StackId": {
            "type": "string",
            "description": "The id of the stack the application will belong to."
        },
        "Type": {
            "type": "string",
            "description": "Opsworks application type. One of `aws-flow-ruby`, `java`, `rails`, `php`, `nodejs`, `static` or `other`."
        },
        "AppSource": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/AppSourceDefinition"
            }
        },
        "Environment": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/EnvironmentDefinition"
            }
        },
        "SslConfiguration": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/SslConfigurationDefinition"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "StackId",
        "Type"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    },
    "writeOnlyProperties": [
        "/definitions/AppSourceDefinition/Password",
        "/definitions/AppSourceDefinition/SshKey",
        "/definitions/SslConfigurationDefinition/PrivateKey"
    ]
}