                    },
                    "maxItems": 1,
                    "minItems": 1
                }
            },
            "required": []
        },
        "S3BucketDestinationDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "BucketAccountId": {
                    "type": "string",
                    "description": "The account ID that owns the destination bucket."
                },
                "BucketArn": {
                    "type": "string",
                    "description": "The ARN of the destination bucket."
                },
                "Format": {
                    "type": "string",
                    "description": "The output format of exported analytics data. Allowed values: `CSV`. Default value: `CSV`."
                },
                "Prefix": {
                    "type": "string",
                    "description": "The prefix to append to exported analytics data."
                }
            },
            "required": [
                "BucketArn"
            ]
        }
    },
    "properties": {
