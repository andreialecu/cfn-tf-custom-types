{
    "typeName": "TF::AWS::S3BucketAnalyticsConfiguration",
    "description": "Provides a S3 bucket [analytics configuration](https://docs.aws.amazon.com/AmazonS3/latest/dev/analytics-storage-class.html) resource.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/aws/TF-AWS-S3BucketAnalyticsConfiguration/docs/README.md",
    "definitions": {
        "FilterDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Prefix": {
                    "type": "string",
                    "description": "Object prefix for filtering."
                },
                "Tags": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/TagsDefinition"
                    },
                    "description": "Set of object tags for filtering."
                }
            },
            "required": []
        },
        "TagsDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "MapKey": {
                    "type": "string"
                },
                "MapValue": {
                    "type": "string"
                }
            },
            "required": [
                "MapKey",
                "MapValue"
            ]
        },
        "StorageClassAnalysisDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "DataExport": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/DataExportDefinition"
                    },
                    "maxItems": 1,
                    "minItems": 1
                }
            },
            "required": []
        },
        "DataExportDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "OutputSchemaVersion": {
                    "type": "string",
                    "description": "The schema version of exported analytics data. Allowed values: `V_1`. Default value: `V_1`."
                },
                "Destination": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/DestinationDefinition"
                    },
                    "maxItems": 1,
                    "minItems": 1
                }
            },
            "required": []
        },
        "DestinationDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "S3BucketDestination": {
                    "type": "array",
                    "insertionOrder": true,
