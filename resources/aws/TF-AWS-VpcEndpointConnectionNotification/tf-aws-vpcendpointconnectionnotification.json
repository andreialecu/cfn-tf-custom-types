{
    "typeName": "TF::AWS::VpcEndpointConnectionNotification",
    "description": "Provides a VPC Endpoint connection notification resource.\nConnection notifications notify subscribers of VPC Endpoint events.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/aws/TF-AWS-VpcEndpointConnectionNotification/docs/README.md",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "ConnectionEvents": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "One or more endpoint [events](https://docs.aws.amazon.com/AWSEC2/latest/APIReference/API_CreateVpcEndpointConnectionNotification.html#API_CreateVpcEndpointConnectionNotification_RequestParameters) for which to receive notifications."
        },
        "ConnectionNotificationArn": {
            "type": "string",
            "description": "The ARN of the SNS topic for the notifications."
        },
        "Id": {
            "type": "string"
        },
        "NotificationType": {
            "type": "string"
        },
        "State": {
            "type": "string"
        },
        "VpcEndpointId": {
            "type": "string",
            "description": "The ID of the VPC Endpoint to receive notifications for."
        },
        "VpcEndpointServiceId": {
            "type": "string",
            "description": "The ID of the VPC Endpoint Service to receive notifications for."
        }
    },
    "additionalProperties": false,
    "required": [
        "ConnectionEvents",
        "ConnectionNotificationArn"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id",
        "/properties/NotificationType",
        "/properties/State"
    ],
    "primaryIdentifier": [
