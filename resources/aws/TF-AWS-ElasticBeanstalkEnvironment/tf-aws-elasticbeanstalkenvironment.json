{
    "typeName": "TF::AWS::ElasticBeanstalkEnvironment",
    "description": "Provides an Elastic Beanstalk Environment Resource. Elastic Beanstalk allows\nyou to deploy and manage applications in the AWS cloud without worrying about\nthe infrastructure that runs those applications.\n\nEnvironments are often things such as `development`, `integration`, or\n`production`.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/aws/TF-AWS-ElasticBeanstalkEnvironment/docs/README.md",
    "definitions": {
        "AllSettingsDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Name": {
                    "type": "string"
                },
                "Namespace": {
                    "type": "string"
                },
                "Resource": {
                    "type": "string"
                },
                "Value": {
                    "type": "string"
                }
            }
        },
        "TagsDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "MapKey": {
                    "type": "string"
                },
                "MapValue": {
                    "type": "string"
                }
            },
            "required": [
                "MapKey",
                "MapValue"
            ]
        },
        "TagsAllDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "MapKey": {
                    "type": "string"
                },
                "MapValue": {
                    "type": "string"
                }
            },
            "required": [
                "MapKey",
                "MapValue"
            ]
        },
        "SettingDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Name": {
                    "type": "string"
                },
                "Namespace": {
                    "type": "string"
                },
                "Resource": {
                    "type": "string"
                },
                "Value": {
                    "type": "string"
                }
            },
            "required": [
                "Name",
                "Namespace",
                "Value"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AllSettings": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/AllSettingsDefinition"
            }
        },
        "Application": {
            "type": "string"
        },
        "Arn": {
            "type": "string"
        },
        "AutoscalingGroups": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "Cname": {
            "type": "string"
        },
        "CnamePrefix": {
            "type": "string",
            "description": "Prefix to use for the fully qualified DNS name of\nthe Environment."
        },
        "Description": {
            "type": "string",
            "description": "Short description of the Environment."
        },
        "EndpointUrl": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Instances": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "LaunchConfigurations": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "LoadBalancers": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "Name": {
            "type": "string",
            "description": "A unique name for this Environment. This name is used\nin the application URL."
        },
        "PlatformArn": {
            "type": "string"
        },
        "PollInterval": {
            "type": "string"
        },
        "Queues": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "SolutionStackName": {
            "type": "string"
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/TagsDefinition"
            },
            "description": "A set of tags to apply to the Environment. If configured with a provider [`default_tags` configuration block](https://www.terraform.io/docs/providers/aws/index.html#default_tags-configuration-block) present, tags with matching keys will overwrite those defined at the provider-level."
        },
        "TagsAll": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/TagsAllDefinition"
            }
        },
        "TemplateName": {
            "type": "string"
        },
        "Tier": {
            "type": "string",
            "description": "Elastic Beanstalk Environment tier. Valid values are `Worker`\nor `WebServer`. If tier is left blank `WebServer` will be used."
        },
        "Triggers": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "VersionLabel": {
            "type": "string",
            "description": "The name of the Elastic Beanstalk Application Version\nto use in deployment."
        },
        "WaitForReadyTimeout": {
            "type": "string",
            "description": "The maximum\n[duration](https://golang.org/pkg/time/#ParseDuration) that Terraform should\nwait for an Elastic Beanstalk Environment to be in a ready state before timing\nout."
        },
        "Setting": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/SettingDefinition"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "Application",
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/AllSettings",
        "/properties/Arn",
        "/properties/AutoscalingGroups",
        "/properties/Cname",
        "/properties/EndpointUrl",
        "/properties/Id",
        "/properties/Instances",
        "/properties/LaunchConfigurations",
        "/properties/LoadBalancers",
        "/properties/Queues",
        "/properties/Triggers"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    }
}