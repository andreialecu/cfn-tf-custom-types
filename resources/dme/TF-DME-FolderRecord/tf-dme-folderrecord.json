{
    "typeName": "TF::DME::FolderRecord",
    "description": "CloudFormation equivalent of dme_folder_record",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/dme/TF-DME-FolderRecord/docs/README.md",
    "definitions": {
        "FolderPermissionsDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "GroupId": {
                    "type": "number"
                },
                "GroupName": {
                    "type": "string"
                },
                "Permission": {
                    "type": "number"
                }
            },
            "required": [
                "GroupId"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "DefaultFolder": {
            "type": "boolean"
        },
        "Domains": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "Secondaries": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "FolderPermissions": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/FolderPermissionsDefinition"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    }
}