    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AccountId": {
            "type": "number",
            "description": "Determines the New Relic account where the dashboard will be created. Defaults to the account associated with the API key used.\n* `description` - (Optional) Brief text describing the dashboard.\n* `permissions` - (Optional) Determines who can see the dashboard in an account. Valid values are `private`, `public_read_only`, or `public_read_write`.  Defaults to `public_read_only`."
        },
        "Description": {
            "type": "string",
            "description": "Brief text describing the dashboard.\n* `permissions` - (Optional) Determines who can see the dashboard in an account. Valid values are `private`, `public_read_only`, or `public_read_write`.  Defaults to `public_read_only`."
        },
        "Guid": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The title of the dashboard.\n* `page` - (Required) A nested block that describes a page. See [Nested page blocks](#nested-page-blocks) below for details.\n* `account_id` - (Optional) Determines the New Relic account where the dashboard will be created. Defaults to the account associated with the API key used.\n* `description` - (Optional) Brief text describing the dashboard.\n* `permissions` - (Optional) Determines who can see the dashboard in an account. Valid values are `private`, `public_read_only`, or `public_read_write`.  Defaults to `public_read_only`."
        },
        "Permalink": {
            "type": "string"
        },
        "Permissions": {
            "type": "string",
            "description": "Determines who can see the dashboard in an account. Valid values are `private`, `public_read_only`, or `public_read_write`.  Defaults to `public_read_only`."
