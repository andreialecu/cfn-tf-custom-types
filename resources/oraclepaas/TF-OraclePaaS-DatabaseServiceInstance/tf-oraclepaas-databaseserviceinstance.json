        "/definitions/HybridDisasterRecoveryDefinition/CloudStoragePassword",
        "/definitions/InstantiateFromBackupDefinition/CloudStoragePassword",
        "/definitions/InstantiateFromBackupDefinition/CloudStorageUsername"
    ]
}