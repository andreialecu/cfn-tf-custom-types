    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AvailabilityDomain": {
            "type": "string",
            "description": "Name of the availability domain within the region where the Oracle Database Cloud Service instance is to be provisioned."
        },
        "BringYourOwnLicense": {
            "type": "boolean",
            "description": "Specify if you want to use an existing perpetual license to Oracle Database to establish the right to use Oracle Database on the new instance.\nDefault value is `false`."
        },
        "CloudStorageContainer": {
            "type": "string"
        },
        "ComputeSiteName": {
            "type": "string"
        },
        "DbaasMonitorUrl": {
            "type": "string"
        },
        "Description": {
            "type": "string",
            "description": "A description of the Service Instance."
        },
        "DesiredState": {
            "type": "string",
            "description": "Specifies the desired state of the service instance. Allowed values are `start`, `stop`,\nand `restart`."
        },
        "Edition": {
            "type": "string",
            "description": "Database edition for the service instance. Possible values are `SE`, `EE`, `EE_HP`, or `EE_EP`."
        },
        "EmUrl": {
            "type": "string"
        },
        "GlassfishUrl": {
            "type": "string"
        },
        "HighPerformanceStorage": {
            "type": "boolean",
            "description": "Specifies whether the service instance will be provisioned with high performance storage.\nDefault value is `false`."
        },
        "Id": {
            "type": "string"
        },
        "IdentityDomain": {
            "type": "string"
        },
        "IpNetwork": {
            "type": "string",
            "description": "This attribute is only applicable to accounts where regions are supported. The three-part name of an IP network to which the service instance is added. For example: /Compute-identity_domain/user/object."
        },
        "IpReservations": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "Groups one or more IP reservations in use on this service instance. This attribute is only applicable to accounts where regions are supported."
        },
        "Level": {
            "type": "string",
            "description": "Service level for the service instance. Possible values are `BASIC` or `PAAS`."
        },
        "Name": {
            "type": "string",
            "description": "The name of the Service Instance."
        },
        "NotificationEmail": {
            "type": "string",
            "description": "The email address to send notifications around successful or unsuccessful completions of the instance-creation operation."
        },
        "Region": {
            "type": "string",
            "description": "Specifies the location where the service instance is provisioned (only for accounts where regions are supported)."
        },
        "Shape": {
            "type": "string",
            "description": "Desired compute shape. Possible values are `oc3`, `oc4`, `oc5`, `oc6`, `oc1m`, `oc2m`, `oc3m`, or `oc4m`."
        },
        "SshPublicKey": {
            "type": "string"
        },
        "Status": {
            "type": "string"
        },
        "Subnet": {
            "type": "string",
            "description": "Name of the subnet within the region where the Oracle Database Cloud Service instance is to be provisioned."
        },
        "SubscriptionType": {
            "type": "string",
            "description": "Billing unit. Possible values are `HOURLY` or `MONTHLY`."
        },
        "Uri": {
            "type": "string"
        },
        "Version": {
            "type": "string",
            "description": "Oracle Database software version; one of: `12.2.0.1`, `12.1.0.2`, or `11.2.0.4`."
        },
        "Backups": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/BackupsDefinition"
            },
            "maxItems": 1
        },
        "DatabaseConfiguration": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/DatabaseConfigurationDefinition"
            },
            "maxItems": 1,
            "minItems": 1
        },
        "DefaultAccessRules": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/DefaultAccessRulesDefinition"
            },
            "maxItems": 1
        },
        "HybridDisasterRecovery": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/HybridDisasterRecoveryDefinition"
            },
            "maxItems": 1
        },
        "InstantiateFromBackup": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/InstantiateFromBackupDefinition"
            },
            "maxItems": 1
        },
        "Standby": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/StandbyDefinition"
            },
            "maxItems": 1
        },
        "Timeouts": {
            "$ref": "#/definitions/TimeoutsDefinition"
        }
    },
    "additionalProperties": false,
    "required": [
        "Edition",
        "Name",
        "Shape",
        "SshPublicKey",
        "SubscriptionType",
        "Version"
    ],
