{
    "typeName": "TF::OraclePaaS::DatabaseServiceInstance",
    "description": "The `oraclepaas_database_service_instance` resource creates and manages a an Oracle Database Cloud Service instance on the Oracle Cloud Platform.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/oraclepaas/TF-OraclePaaS-DatabaseServiceInstance/docs/README.md",
