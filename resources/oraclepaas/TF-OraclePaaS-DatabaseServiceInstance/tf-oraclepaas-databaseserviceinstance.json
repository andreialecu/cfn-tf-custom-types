                },
                "CloudStorageUsername": {
                    "type": "string"
                },
                "CreateIfMissing": {
                    "type": "boolean"
                }
            },
            "required": [
                "CloudStorageContainer"
            ]
        },
        "DatabaseConfigurationDefinition": {
            "type": "object",
