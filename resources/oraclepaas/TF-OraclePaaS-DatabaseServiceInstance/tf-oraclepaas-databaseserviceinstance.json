                },
                "DataStorageVolumeSize": {
                    "type": "number"
                },
                "DbDemo": {
                    "type": "string"
                },
                "DisasterRecovery": {
                    "type": "boolean"
                },
                "FailoverDatabase": {
                    "type": "boolean"
                },
                "GoldenGate": {
                    "type": "boolean"
