{
    "typeName": "TF::Icinga2::Checkcommand",
    "description": "Configures an Icinga2 checkcommand resource. This allows checkcommands to be configured, updated,\nand deleted.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/icinga2/TF-Icinga2-Checkcommand/docs/README.md",
    "definitions": {
        "ArgumentsDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "MapKey": {
                    "type": "string"
                },
                "MapValue": {
                    "type": "string"
                }
            },
            "required": [
                "MapKey",
                "MapValue"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Arguments": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/ArgumentsDefinition"
            },
            "description": "A mapping of arguments to include with the command."
        },
        "Command": {
            "type": "string",
            "description": "Path to the command te be executed."
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "Name by which to reference the checkcommand."
        },
        "Templates": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "A list of Icinga2 templates to assign to the host."
        }
    },
    "additionalProperties": false,
    "required": [
        "Command",
        "Name",
        "Templates"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    }
}