        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AppId": {
            "type": "number",
            "description": "The id of the App resource to which the role should belong."
        },
        "Id": {
            "type": "string"
        },
        "RoleId": {
            "type": "number",
            "description": "The id of the Role being attached to the App."
        }
    },
    "additionalProperties": false,
