        },
        "ArpRetry": {
            "type": "number",
            "description": "Number of additional gratuitous ARPs sent out after HA failover (1-255, default is 4)."
        },
        "DeadTimer": {
            "type": "number",
            "description": "VRRP-A dead timer in terms of how many hello messages missed, default is 5 (2-255, default is 5)."
