{
    "typeName": "TF::OpsGenie::IncidentTemplate",
    "description": "Manages an Incident Template within Opsgenie.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/opsgenie/TF-OpsGenie-IncidentTemplate/docs/README.md",
