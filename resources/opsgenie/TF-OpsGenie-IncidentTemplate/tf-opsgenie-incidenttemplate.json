        "DetailsDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "MapKey": {
                    "type": "string"
                },
                "MapValue": {
                    "type": "string"
                }
            },
            "required": [
                "MapKey",
                "MapValue"
            ]
        },
        "StakeholderPropertiesDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Description": {
                    "type": "string"
                },
                "Enable": {
                    "type": "boolean"
                },
                "Message": {
                    "type": "string"
                }
            },
            "required": [
                "Message"
            ]
        }
    },
    "properties": {
