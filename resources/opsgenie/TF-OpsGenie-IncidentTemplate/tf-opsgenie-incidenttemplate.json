        },
        "Description": {
            "type": "string"
        },
        "Details": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/DetailsDefinition"
            }
        },
        "Id": {
            "type": "string"
        },
        "ImpactedServices": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "Message": {
            "type": "string"
        },
        "Name": {
            "type": "string"
        },
        "Priority": {
            "type": "string"
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "StakeholderProperties": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/StakeholderPropertiesDefinition"
            },
            "minItems": 1
        }
    },
    "additionalProperties": false,
    "required": [
        "Message",
        "Name",
        "Priority"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
