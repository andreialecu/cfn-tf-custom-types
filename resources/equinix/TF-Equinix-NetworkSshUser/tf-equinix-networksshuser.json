{
    "typeName": "TF::Equinix::NetworkSshUser",
    "description": "Resource `equinix_network_ssh_user` allows creation and management of Equinix Network\nEdge SSH users.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/equinix/TF-Equinix-NetworkSshUser/docs/README.md",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "DeviceIds": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "list of device identifiers to which user will have access."
        },
        "Id": {
            "type": "string"
        },
        "Password": {
            "type": "string",
            "description": "SSH user password."
        },
        "Username": {
            "type": "string",
            "description": "SSH user login name."
        },
        "Uuid": {
            "type": "string"
        }
    },
    "additionalProperties": false,
