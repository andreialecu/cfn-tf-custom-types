                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "ByoipId": {
            "type": "string",
            "description": "The OCID of the Byoip Range Id object to which the cidr block belongs."
        },
        "CidrBlock": {
            "type": "string",
            "description": "The CIDR IP address range to be added to the Public Ip Pool. Example: `10.0.1.0/24`."
        },
        "Id": {
