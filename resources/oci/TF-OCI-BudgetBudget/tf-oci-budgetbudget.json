        "Amount": {
            "type": "number",
            "description": "(Updatable) The amount of the budget expressed as a whole number in the currency of the customer's rate card."
        },
        "BudgetProcessingPeriodStartOffset": {
            "type": "number",
            "description": "(Updatable) The number of days offset from the first day of the month, at which the budget processing period starts. In months that have fewer days than this value, processing will begin on the last day of that month. For example, for a value of 12, processing starts every month on the 12th at midnight."
        },
        "CompartmentId": {
            "type": "string",
