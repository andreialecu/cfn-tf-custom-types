    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AdminPassword": {
            "type": "string",
            "description": "The password for the administrative user. The password must be between 8 and 32 characters long, and must contain at least 1 numeric character, 1 lowercase character, 1 uppercase character, and 1 special (nonalphanumeric) character."
        },
        "AdminUsername": {
            "type": "string",
            "description": "The username for the administrative user."
        },
        "AnalyticsCluster": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/AnalyticsClusterDefinition"
            }
        },
        "AvailabilityDomain": {
            "type": "string",
            "description": "The availability domain on which to deploy the Read/Write endpoint. This defines the preferred primary instance."
        },
        "Channels": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/ChannelsDefinition6"
            }
        },
        "CompartmentId": {
            "type": "string",
            "description": "The OCID of the compartment."
        },
        "ConfigurationId": {
            "type": "string",
            "description": "The OCID of the Configuration to be used for this DB System."
        },
        "CurrentPlacement": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/CurrentPlacementDefinition"
            }
        },
        "DataStorageSizeInGb": {
            "type": "number",
            "description": "Initial size of the data volume in GBs that will be created and attached. Keep in mind that this only specifies the size of the database data volume, the log volume for the database will be scaled appropriately with its shape. It is required if you are creating a new database. It cannot be set if you are creating a database from a backup."
        },
        "DefinedTags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/DefinedTagsDefinition"
            },
            "description": "(Updatable) Usage of predefined tag keys. These predefined keys are scoped to namespaces. Example: `{\"foo-namespace.bar-key\": \"value\"}`."
        },
        "Description": {
            "type": "string",
            "description": "(Updatable) User-provided data about the DB System."
        },
        "DisplayName": {
            "type": "string",
            "description": "(Updatable) The user-friendly name for the DB System. It does not have to be unique."
        },
        "Endpoints": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/EndpointsDefinition"
            }
        },
        "FaultDomain": {
            "type": "string",
            "description": "The fault domain on which to deploy the Read/Write endpoint. This defines the preferred primary instance."
        },
        "FreeformTags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/FreeformTagsDefinition"
            },
            "description": "(Updatable) Simple key-value pair applied without any predefined name, type or scope. Exists for cross-compatibility only. Example: `{\"bar-key\": \"value\"}`."
        },
        "HeatWaveCluster": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/HeatWaveClusterDefinition"
            }
        },
        "HostnameLabel": {
            "type": "string",
            "description": "The hostname for the primary endpoint of the DB System. Used for DNS."
        },
        "Id": {
            "type": "string"
        },
        "IpAddress": {
            "type": "string",
            "description": "The IP address the DB System is configured to listen on. A private IP address of your choice to assign to the primary endpoint of the DB System. Must be an available IP address within the subnet's CIDR. If you don't specify a value, Oracle automatically assigns a private IP address from the subnet. This should be a \"dotted-quad\" style IPv4 address."
        },
        "IsAnalyticsClusterAttached": {
            "type": "boolean"
        },
        "IsHeatWaveClusterAttached": {
            "type": "boolean"
        },
        "IsHighlyAvailable": {
            "type": "boolean",
            "description": "(Updatable) Specifies if the DB System is highly available."
        },
        "LifecycleDetails": {
            "type": "string"
        },
        "MysqlVersion": {
            "type": "string"
        },
        "Port": {
            "type": "number",
            "description": "The port for primary endpoint of the DB System to listen on."
        },
        "PortX": {
            "type": "number",
            "description": "The TCP network port on which X Plugin listens for connections. This is the X Plugin equivalent of port."
        },
        "ShapeName": {
            "type": "string",
            "description": "The name of the shape. The shape determines the resources allocated\n* CPU cores and memory for VM shapes; CPU cores, memory and storage for non-VM (or bare metal) shapes. To get a list of shapes, use the [ListShapes](https://docs.cloud.oracle.com/iaas/api/#/en/mysql/20190415/ShapeSummary/ListShapes) operation."
        },
        "ShutdownType": {
            "type": "string",
            "description": "It is applicable only for stopping a DB System. Could be set to `FAST`, `SLOW` or `IMMEDIATE`. Default value is `FAST`."
        },
        "State": {
            "type": "string",
            "description": "(Updatable) The target state for the DB System. Could be set to `ACTIVE` or `INACTIVE`."
        },
        "SubnetId": {
            "type": "string",
            "description": "The OCID of the subnet the DB System is associated with."
        },
        "TimeCreated": {
            "type": "string"
        },
        "TimeUpdated": {
            "type": "string"
        },
        "BackupPolicy": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/BackupPolicyDefinition"
            },
            "maxItems": 1
        },
        "Maintenance": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/MaintenanceDefinition"
            },
            "maxItems": 1
        },
        "Source": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/SourceDefinition"
            },
            "maxItems": 1
        },
        "Timeouts": {
            "$ref": "#/definitions/TimeoutsDefinition"
        }
    },
