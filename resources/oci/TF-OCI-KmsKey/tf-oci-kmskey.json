        },
        "TimeOfDeletion": {
            "type": "string",
            "description": "(Updatable) An optional property for the deletion time of the key, expressed in [RFC 3339](https://tools.ietf.org/html/rfc3339) timestamp format. Example: `2019-04-03T21:10:29.600Z`."
        },
        "VaultId": {
