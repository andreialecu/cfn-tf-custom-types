{
    "typeName": "TF::OCI::AnalyticsAnalyticsInstanceVanityUrl",
    "description": "This resource provides the Analytics Instance Vanity Url resource in Oracle Cloud Infrastructure Analytics service.\n\nAllows specifying a custom host name to be used to access the analytics instance.  This requires prior setup of DNS entry and certificate\nfor this host.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/oci/TF-OCI-AnalyticsAnalyticsInstanceVanityUrl/docs/README.md",
    "definitions": {
        "TimeoutsDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AnalyticsInstanceId": {
            "type": "string",
            "description": "The OCID of the AnalyticsInstance."
        },
        "CaCertificate": {
            "type": "string",
            "description": "(Updatable) PEM CA certificate(s) for HTTPS connections. This may include multiple PEM certificates."
        },
        "Description": {
            "type": "string",
            "description": "Optional description."
        },
        "Hosts": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "List of fully qualified hostnames supported by this vanity URL definition (max of 3)."
        },
        "Id": {
            "type": "string"
        },
        "Passphrase": {
            "type": "string",
            "description": "(Updatable) Passphrase for the PEM Private key (if any)."
        },
        "PrivateKey": {
            "type": "string",
            "description": "(Updatable) PEM Private key for HTTPS connections."
        },
        "PublicCertificate": {
            "type": "string",
            "description": "(Updatable) PEM certificate for HTTPS connections."
        },
        "Timeouts": {
            "$ref": "#/definitions/TimeoutsDefinition"
        }
    },
    "additionalProperties": false,
    "required": [
        "AnalyticsInstanceId",
        "CaCertificate",
        "Hosts",
        "PrivateKey",
        "PublicCertificate"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    },
    "writeOnlyProperties": [
        "/properties/Passphrase",
        "/properties/PrivateKey"
    ]
}