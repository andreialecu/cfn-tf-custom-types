{
    "typeName": "TF::OCI::LoadBalancerListener",
    "description": "This resource provides the Listener resource in Oracle Cloud Infrastructure Load Balancer service.\n\nAdds a listener to a load balancer.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/oci/TF-OCI-LoadBalancerListener/docs/README.md",
    "definitions": {
        "ConnectionConfigurationDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "BackendTcpProxyProtocolVersion": {
                    "type": "number"
                },
                "IdleTimeoutInSeconds": {
                    "type": "string"
                }
            },
            "required": [
                "IdleTimeoutInSeconds"
            ]
        },
        "SslConfigurationDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "CertificateName": {
                    "type": "string"
                },
                "CipherSuiteName": {
                    "type": "string"
                },
                "Protocols": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "type": "string"
                    }
                },
                "ServerOrderPreference": {
                    "type": "string"
                },
                "VerifyDepth": {
                    "type": "number"
                },
                "VerifyPeerCertificate": {
                    "type": "boolean"
                }
            },
            "required": [
                "CertificateName"
            ]
        },
        "TimeoutsDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "DefaultBackendSetName": {
            "type": "string",
            "description": "(Updatable) The name of the associated backend set.  Example: `example_backend_set`."
        },
        "HostnameNames": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "(Updatable) An array of hostname resource names."
        },
        "Id": {
            "type": "string"
        },
        "LoadBalancerId": {
            "type": "string",
            "description": "The [OCID](https://docs.cloud.oracle.com/iaas/Content/General/Concepts/identifiers.htm) of the load balancer on which to add a listener."
        },
        "Name": {
            "type": "string",
            "description": "A friendly name for the listener. It must be unique and it cannot be changed. Avoid entering confidential information.  Example: `example_listener`."
        },
        "PathRouteSetName": {
            "type": "string",
            "description": "(Updatable) Deprecated. Please use `routingPolicies` instead."
        },
        "Port": {
            "type": "number",
            "description": "(Updatable) The communication port for the listener.  Example: `80`."
        },
        "Protocol": {
            "type": "string",
            "description": "(Updatable) The protocol on which the listener accepts connection requests. To get a list of valid protocols, use the [ListProtocols](https://docs.cloud.oracle.com/iaas/api/#/en/loadbalancer/20170115/LoadBalancerProtocol/ListProtocols) operation.  Example: `HTTP`."
        },
        "RoutingPolicyName": {
            "type": "string",
            "description": "(Updatable) The name of the routing policy applied to this listener's traffic.  Example: `example_routing_policy`."
        },
        "RuleSetNames": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "(Updatable) The names of the [rule sets](https://docs.cloud.oracle.com/iaas/api/#/en/loadbalancer/20170115/RuleSet/) to apply to the listener.  Example: [\"example_rule_set\"]."
        },
        "State": {
            "type": "string"
        },
        "ConnectionConfiguration": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/ConnectionConfigurationDefinition"
            },
            "maxItems": 1
        },
        "SslConfiguration": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/SslConfigurationDefinition"
            },
            "maxItems": 1
        },
        "Timeouts": {
            "$ref": "#/definitions/TimeoutsDefinition"
        }
    },
    "additionalProperties": false,
    "required": [
        "DefaultBackendSetName",
        "LoadBalancerId",
        "Name",
        "Port",
        "Protocol"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id",
        "/properties/State"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    }
}