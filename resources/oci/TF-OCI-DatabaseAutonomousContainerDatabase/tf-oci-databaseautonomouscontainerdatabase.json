        "PeerAutonomousVmClusterId": {
            "type": "string",
            "description": "The [OCID](https://docs.cloud.oracle.com/iaas/Content/General/Concepts/identifiers.htm) of the peer Autonomous VM cluster for Autonomous Data Guard. Required to enable Data Guard."
        },
        "PeerDbUniqueName": {
            "type": "string",
            "description": "The `DB_UNIQUE_NAME` of the peer Autonomous Container Database in a Data Guard association."
        },
        "ProtectionMode": {
            "type": "string",
            "description": "The protection mode of this Autonomous Data Guard association. For more information, see [Oracle Data Guard Protection Modes](http://docs.oracle.com/database/122/SBYDB/oracle-data-guard-protection-modes.htm#SBYDB02000) in the Oracle Data Guard documentation."
        },
        "Role": {
            "type": "string"
        },
        "RotateKeyTrigger": {
            "type": "boolean",
            "description": "(Updatable) An optional property when flipped triggers rotation of KMS key. It is only applicable on dedicated container databases i.e. where `autonomous_exadata_infrastructure_id` is set."
        },
        "ServiceLevelAgreementType": {
            "type": "string",
            "description": "The service level agreement type of the Autonomous Container Database. The default is STANDARD. For an autonomous dataguard Autonomous Container Database, the specified Autonomous Exadata Infrastructure must be associated with a remote Autonomous Exadata Infrastructure."
        },
        "StandbyMaintenanceBufferInDays": {
            "type": "number",
            "description": "(Updatable) The scheduling detail for the quarterly maintenance window of standby Autonomous Container Database. This value represents the number of days before the primary database maintenance schedule."
        },
