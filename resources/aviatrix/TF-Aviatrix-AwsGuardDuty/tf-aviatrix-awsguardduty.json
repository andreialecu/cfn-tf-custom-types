    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AccountName": {
            "type": "string"
        },
        "ExcludedIps": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "Id": {
            "type": "string"
        },
        "Region": {
            "type": "string"
        }
    },
    "additionalProperties": false,
