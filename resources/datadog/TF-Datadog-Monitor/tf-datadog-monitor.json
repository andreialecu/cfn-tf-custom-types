        "Validate": {
            "type": "boolean"
        },
        "MonitorThresholdWindows": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/MonitorThresholdWindowsDefinition"
            },
            "maxItems": 1
