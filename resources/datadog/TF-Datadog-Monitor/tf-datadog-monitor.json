{
    "typeName": "TF::Datadog::Monitor",
    "description": "CloudFormation equivalent of datadog_monitor",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/datadog/TF-Datadog-Monitor/docs/README.md",
    "definitions": {
        "MonitorThresholdWindowsDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "RecoveryWindow": {
                    "type": "string"
                },
                "TriggerWindow": {
                    "type": "string"
                }
            },
            "required": []
        },
        "MonitorThresholdsDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Critical": {
                    "type": "string"
                },
                "CriticalRecovery": {
                    "type": "string"
                },
                "Ok": {
                    "type": "string"
