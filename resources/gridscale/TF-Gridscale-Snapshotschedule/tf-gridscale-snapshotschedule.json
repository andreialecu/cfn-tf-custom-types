{
    "typeName": "TF::Gridscale::Snapshotschedule",
    "description": "Provides a storage snapshot schedule resource. This can be used to create, modify, and delete snapshot schedules.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/gridscale/TF-Gridscale-Snapshotschedule/docs/README.md",
    "definitions": {
        "SnapshotDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "CreateTime": {
                    "type": "string"
                },
                "Name": {
                    "type": "string"
                },
                "ObjectUuid": {
                    "type": "string"
                }
            }
        },
        "TimeoutsDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "ChangeTime": {
            "type": "string"
        },
        "CreateTime": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "KeepSnapshots": {
            "type": "number",
            "description": "The amount of Snapshots to keep before overwriting the last created Snapshot (>=1)."
        },
        "Labels": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "The list of labels."
        },
        "Name": {
            "type": "string",
            "description": "UUID of the snapshot schedule."
        },
        "NextRuntime": {
            "type": "string",
            "description": "The date and time that the snapshot schedule will be run."
        },
        "NextRuntimeComputed": {
            "type": "string"
        },
        "RunInterval": {
            "type": "number",
            "description": "The interval at which the schedule will run (in minutes, >=60)."
        },
        "Snapshot": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/SnapshotDefinition"
            }
        },
        "Status": {
            "type": "string"
        },
        "StorageUuid": {
            "type": "string",
            "description": "UUID of the storage that the snapshot schedule belongs to."
        },
        "Timeouts": {
            "$ref": "#/definitions/TimeoutsDefinition"
        }
    },
    "additionalProperties": false,
    "required": [
        "KeepSnapshots",
        "Name",
        "RunInterval",
        "StorageUuid"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/ChangeTime",
        "/properties/CreateTime",
        "/properties/Id",
        "/properties/NextRuntimeComputed",
        "/properties/Snapshot",
