                },
                "Bucket": {
                    "type": "string"
                },
                "Host": {
                    "type": "string"
                },
                "Object": {
                    "type": "string"
                },
                "Private": {
                    "type": "boolean"
                },
                "SecretKey": {
                    "type": "string"
                }
