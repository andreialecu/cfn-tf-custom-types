{
    "typeName": "TF::Gridscale::StorageImport",
    "description": "Create a new storage via a storage backup. This can be used to create, modify, and delete the imported storages.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/gridscale/TF-Gridscale-StorageImport/docs/README.md",
    "definitions": {
        "TimeoutsDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Capacity": {
            "type": "number",
            "description": "The default value is inherited from the source storage instance. A desired capacity is possible. Required (integer - minimum: 1 - maximum: 4096)."
        },
        "ChangeTime": {
            "type": "string"
        },
        "CreateTime": {
            "type": "string"
        },
        "CurrentPrice": {
            "type": "number"
        },
        "Id": {
            "type": "string"
        },
        "Labels": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "List of labels in the format [ \"label1\", \"label2\" ]."
        },
        "LastUsedTemplate": {
            "type": "string"
        },
        "LicenseProductNo": {
            "type": "number"
        },
        "LocationCountry": {
            "type": "string"
        },
        "LocationIata": {
            "type": "string"
        },
        "LocationName": {
            "type": "string"
        },
        "LocationUuid": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The human-readable name of the object. It supports the full UTF-8 character set, with a maximum of 64 characters."
        },
        "ParentUuid": {
            "type": "string"
        },
        "Status": {
            "type": "string"
        },
        "StorageBackupId": {
            "type": "string",
            "description": "ID of the storage backup that will be used to create a new storage from."
        },
        "StorageType": {
            "type": "string",
            "description": "The default value is inherited from the source storage instance. A desired storage type is possible. (one of storage, storage_high, storage_insane)."
        },
        "UsageInMinutes": {
            "type": "number"
        },
        "Timeouts": {
            "$ref": "#/definitions/TimeoutsDefinition"
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "StorageBackupId"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/ChangeTime",
        "/properties/CreateTime",
        "/properties/CurrentPrice",
        "/properties/Id",
        "/properties/LastUsedTemplate",
        "/properties/LicenseProductNo",
        "/properties/LocationCountry",
        "/properties/LocationIata",
        "/properties/LocationName",
        "/properties/LocationUuid",
