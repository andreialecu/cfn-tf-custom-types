{
    "typeName": "TF::Gridscale::Mariadb",
    "description": "Provides a MariaDB resource. This can be used to create, modify, and delete MariaDB instances.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/gridscale/TF-Gridscale-Mariadb/docs/README.md",
    "definitions": {
        "ListenPortDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Name": {
                    "type": "string"
                },
                "Port": {
                    "type": "number"
                }
            }
        },
        "TimeoutsDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Create": {
                    "type": "string"
                },
                "Delete": {
                    "type": "string"
                },
                "Update": {
                    "type": "string"
                }
            },
            "required": []
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "ChangeTime": {
            "type": "string"
        },
        "CreateTime": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Labels": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "List of labels in the format [ \"label1\", \"label2\" ]."
        },
        "ListenPort": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/ListenPortDefinition"
            }
        },
        "MariadbBinlogFormat": {
            "type": "string",
            "description": "MariaDB parameter: Binary Logging Format. Default: \"MIXED\"."
        },
        "MariadbDefaultTimeZone": {
            "type": "string",
            "description": "MariaDB parameter: Server Timezone. Default: UTC."
        },
        "MariadbLogBin": {
            "type": "boolean",
            "description": "MariaDB parameter: Binary Logging. Default: false."
        },
        "MariadbMaxAllowedPacket": {
            "type": "string",
            "description": "MariaDB parameter: Max Allowed Packet Size. Format: xM (where x is an integer, M stands for unit: k(kB), M(MB), G(GB)). Default: 64M."
        },
        "MariadbMaxConnections": {
            "type": "number",
            "description": "MariaDB parameter: Max Connections. Default: 4000."
        },
        "MariadbQueryCache": {
            "type": "boolean",
            "description": "MariaDB parameter: Enable query cache. Default: true."
        },
        "MariadbQueryCacheLimit": {
            "type": "string",
            "description": "MariaDB parameter: Query Cache Limit. Format: xM (where x is an integer, M stands for unit: k(kB), M(MB), G(GB)). Default: 1M."
        },
        "MariadbQueryCacheSize": {
            "type": "string",
            "description": "MariaDB parameter: Query Cache Size. Format: xM (where x is an integer, M stands for unit: k(kB), M(MB), G(GB)). Default: 128M."
        },
        "MariadbServerId": {
            "type": "number",
            "description": "MariaDB parameter: Server Id. Default: 1."
        },
        "MariadbSqlMode": {
            "type": "string",
            "description": "MariaDB parameter: SQL Mode. Default: \"NO_AUTO_CREATE_USER,NO_ENGINE_SUBSTITUTION,STRICT_TRANS_TABLES,ERROR_FOR_DIVISION_BY_ZERO\"."
        },
        "MaxCoreCount": {
            "type": "number",
            "description": "Maximum CPU core count. The MariaDB instance's CPU core count will be autoscaled based on the workload. The number of cores stays between 1 and `max_core_count`."
        },
        "Name": {
            "type": "string",
            "description": "The human-readable name of the object. It supports the full UTF-8 character set, with a maximum of 64 characters."
        },
        "NetworkUuid": {
            "type": "string"
        },
        "Password": {
            "type": "string"
        },
        "PerformanceClass": {
            "type": "string",
            "description": "Performance class of MariaDB service. Available performance classes at the time of writing: `standard`, `high`, `insane`, `ultra`."
        },
        "Release": {
            "type": "string",
            "description": "The MariaDB release of this instance. For convenience, please use [gscloud](https://github.com/gridscale/gscloud) to get the list of available MariaDB service releases."
        },
        "SecurityZoneUuid": {
            "type": "string",
            "description": "The UUID of the security zone that the service is running in."
        },
        "ServiceTemplateUuid": {
            "type": "string"
        },
        "Status": {
            "type": "string"
        },
        "UsageInMinutes": {
            "type": "number"
        },
        "Username": {
            "type": "string"
        },
        "Timeouts": {
            "$ref": "#/definitions/TimeoutsDefinition"
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "PerformanceClass",
        "Release"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/ChangeTime",
        "/properties/CreateTime",
        "/properties/Id",
        "/properties/ListenPort",
        "/properties/NetworkUuid",
        "/properties/Password",
        "/properties/ServiceTemplateUuid",
        "/properties/Status",
        "/properties/UsageInMinutes",
        "/properties/Username"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    },
    "writeOnlyProperties": [
        "/properties/Password",
        "/properties/Username"
    ]
}