{
    "typeName": "TF::VSphere::DistributedPortGroup",
    "description": "The `vsphere_distributed_port_group` resource can be used to manage vSphere\ndistributed virtual port groups. These port groups are connected to distributed\nvirtual switches, which can be managed by the\n[`vsphere_distributed_virtual_switch`][distributed-virtual-switch] resource.\n\nDistributed port groups can be used as networks for virtual machines, allowing\nVMs to use the networking supplied by a distributed virtual switch (DVS), with\na set of policies that apply to that individual newtork, if desired.\n\nFor an overview on vSphere networking concepts, see [this\npage][ref-vsphere-net-concepts]. For more information on vSphere DVS\nportgroups, see [this page][ref-vsphere-dvportgroup].\n\n[distributed-virtual-switch]: /docs/providers/vsphere/r/distributed_virtual_switch.html\n[ref-vsphere-net-concepts]: https://docs.vmware.com/en/VMware-vSphere/6.5/com.vmware.vsphere.networking.doc/GUID-2B11DBB8-CB3C-4AFF-8885-EFEA0FC562F4.html\n[ref-vsphere-dvportgroup]: https://docs.vmware.com/en/VMware-vSphere/6.5/com.vmware.vsp...",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/vsphere/TF-VSphere-DistributedPortGroup/docs/README.md",
    "definitions": {
        "CustomAttributesDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "MapKey": {
                    "type": "string"
                },
                "MapValue": {
                    "type": "string"
                }
            },
            "required": [
                "MapKey",
                "MapValue"
            ]
        },
        "VlanRangeDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "MaxVlan": {
                    "type": "number"
                },
                "MinVlan": {
                    "type": "number"
                }
            },
            "required": [
                "MaxVlan",
                "MinVlan"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "ActiveUplinks": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "AllowForgedTransmits": {
            "type": "boolean"
        },
        "AllowMacChanges": {
            "type": "boolean"
        },
        "AllowPromiscuous": {
            "type": "boolean"
        },
        "AutoExpand": {
            "type": "boolean",
            "description": "Allows the port group to create additional ports\npast the limit specified in `number_of_ports` if necessary. Default: `true`."
        },
        "BlockAllPorts": {
            "type": "boolean"
        },
        "BlockOverrideAllowed": {
            "type": "boolean"
        },
        "CheckBeacon": {
            "type": "boolean"
        },
        "ConfigVersion": {
            "type": "string"
        },
        "CustomAttributes": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/CustomAttributesDefinition"
            }
        },
        "Description": {
            "type": "string",
            "description": "An optional description for the port group."
        },
        "DirectpathGen2Allowed": {
            "type": "boolean"
        },
        "DistributedVirtualSwitchUuid": {
            "type": "string",
            "description": "The ID of the DVS to add the\nport group to. Forces a new resource if changed."
        },
        "EgressShapingAverageBandwidth": {
            "type": "number"
        },
        "EgressShapingBurstSize": {
            "type": "number"
        },
        "EgressShapingEnabled": {
            "type": "boolean"
        },
        "EgressShapingPeakBandwidth": {
            "type": "number"
        },
        "Failback": {
            "type": "boolean"
        },
        "Id": {
            "type": "string"
        },
        "IngressShapingAverageBandwidth": {
            "type": "number"
        },
        "IngressShapingBurstSize": {
            "type": "number"
        },
        "IngressShapingEnabled": {
            "type": "boolean"
        },
        "IngressShapingPeakBandwidth": {
            "type": "number"
        },
        "Key": {
            "type": "string"
        },
        "LacpEnabled": {
            "type": "boolean"
        },
        "LacpMode": {
            "type": "string"
        },
        "LivePortMovingAllowed": {
            "type": "boolean"
        },
        "Name": {
            "type": "string",
            "description": "The name of the port group."
        },
        "NetflowEnabled": {
            "type": "boolean"
        },
        "NetflowOverrideAllowed": {
            "type": "boolean"
        },
        "NetworkResourcePoolKey": {
            "type": "string",
            "description": "The key of a network resource pool\nto associate with this port group. The default is `-1`, which implies no\nassociation."
        },
        "NetworkResourcePoolOverrideAllowed": {
            "type": "boolean"
        },
        "NotifySwitches": {
            "type": "boolean"
        },
        "NumberOfPorts": {
            "type": "number",
            "description": "The number of ports available on this port\ngroup. Cannot be decreased below the amount of used ports on the port group."
        },
        "PortConfigResetAtDisconnect": {
            "type": "boolean"
        },
        "PortNameFormat": {
            "type": "string",
            "description": "An optional formatting policy for naming of\nthe ports in this port group. See the `portNameFormat` attribute listed\n[here][ext-vsphere-portname-format] for details on the format syntax."
        },
        "PortPrivateSecondaryVlanId": {
            "type": "number"
        },
        "SecurityPolicyOverrideAllowed": {
            "type": "boolean"
        },
        "ShapingOverrideAllowed": {
            "type": "boolean"
        },
        "StandbyUplinks": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "Tags": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "TeamingPolicy": {
            "type": "string"
        },
        "TrafficFilterOverrideAllowed": {
            "type": "boolean"
        },
        "TxUplink": {
            "type": "boolean"
        },
        "Type": {
            "type": "string",
            "description": "The port group type. Can be one of `earlyBinding` (static\nbinding) or `ephemeral`. Default: `earlyBinding`."
        },
        "UplinkTeamingOverrideAllowed": {
            "type": "boolean"
        },
        "VlanId": {
            "type": "number"
        },
        "VlanOverrideAllowed": {
            "type": "boolean"
        },
        "VlanRange": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/VlanRangeDefinition"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "DistributedVirtualSwitchUuid",
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/ConfigVersion",
        "/properties/Id",
        "/properties/Key"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
