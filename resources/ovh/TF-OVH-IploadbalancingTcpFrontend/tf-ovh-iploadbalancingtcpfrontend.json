{
    "typeName": "TF::OVH::IploadbalancingTcpFrontend",
    "description": "Creates a backend server group (frontend) to be used by loadbalancing frontend(s)",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/ovh/TF-OVH-IploadbalancingTcpFrontend/docs/README.md",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AllowedSource": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "Restrict IP Load Balancing access to these ip block. No restriction if null. List of IP blocks."
        },
        "DedicatedIpfo": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "Only attach frontend on these ip. No restriction if null. List of Ip blocks."
        },
        "DefaultFarmId": {
            "type": "number",
            "description": "Default TCP Farm of your frontend."
        },
        "DefaultSslId": {
            "type": "number",
            "description": "Default ssl served to your customer."
        },
        "Disabled": {
            "type": "boolean",
            "description": "Disable your frontend. Default: 'false'."
        },
        "DisplayName": {
            "type": "string",
            "description": "Human readable name for your frontend, this field is for you."
        },
        "Id": {
            "type": "string"
        },
        "Port": {
            "type": "string",
            "description": "Port(s) attached to your frontend. Supports single port (numerical value),\nrange (2 dash-delimited increasing ports) and comma-separated list of 'single port'\nand/or 'range'. Each port must be in the [1;49151] range."
        },
        "ServiceName": {
            "type": "string",
            "description": "The internal name of your IP load balancing."
        },
        "Ssl": {
            "type": "boolean",
            "description": "SSL deciphering. Default: 'false'."
        },
        "Zone": {
            "type": "string",
            "description": "Zone where the frontend will be defined (ie. `gra`, `bhs` also supports `all`)."
        }
    },
    "additionalProperties": false,
    "required": [
        "Port",
        "ServiceName",
        "Zone"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    }
}