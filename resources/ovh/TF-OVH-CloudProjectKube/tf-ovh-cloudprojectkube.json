{
    "typeName": "TF::OVH::CloudProjectKube",
    "description": "Creates a kubernetes managed cluster in a public cloud project.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/ovh/TF-OVH-CloudProjectKube/docs/README.md",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "ControlPlaneIsUpToDate": {
            "type": "boolean"
        },
        "Id": {
            "type": "string"
        },
        "IsUpToDate": {
            "type": "boolean"
        },
        "Kubeconfig": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The name of the kubernetes cluster."
        },
        "NextUpgradeVersions": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            }
        },
        "NodesUrl": {
            "type": "string"
        },
        "PrivateNetworkId": {
            "type": "string",
            "description": "OpenStack private network (or vrack) ID to use.\nChanging this value recreates the resource. Defaults - not use private network."
        },
        "Region": {
            "type": "string",
            "description": "a valid OVH public cloud region ID in which the kubernetes\ncluster will be available. Ex.: \"GRA1\". Defaults to all public cloud regions.\nChanging this value recreates the resource."
        },
        "ServiceName": {
            "type": "string",
            "description": "The id of the public cloud project. If omitted,\nthe `OVH_CLOUD_PROJECT_SERVICE` environment variable is used."
        },
        "Status": {
            "type": "string"
        },
        "UpdatePolicy": {
            "type": "string"
        },
        "Url": {
            "type": "string"
        },
        "Version": {
            "type": "string",
            "description": "kubernetes version to use.\nChanging this value recreates the resource. Defaults to latest available."
        }
    },
    "additionalProperties": false,
    "required": [
        "Region",
        "ServiceName"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/ControlPlaneIsUpToDate",
        "/properties/Id",
        "/properties/IsUpToDate",
        "/properties/Kubeconfig",
        "/properties/NextUpgradeVersions",
        "/properties/NodesUrl",
        "/properties/Status",
        "/properties/UpdatePolicy",
        "/properties/Url"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    },
    "writeOnlyProperties": [
        "/properties/Kubeconfig"
    ]
}