{
    "typeName": "TF::OVH::CloudProjectNetworkPrivate",
    "description": "Creates a private network in a public cloud project.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/ovh/TF-OVH-CloudProjectNetworkPrivate/docs/README.md",
    "definitions": {
        "RegionsStatusDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Region": {
                    "type": "string"
                },
                "Status": {
                    "type": "string"
                }
            }
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The name of the network."
        },
        "Regions": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "an array of valid OVH public cloud region ID in which the network\nwill be available. Ex.: \"GRA1\". Defaults to all public cloud regions."
        },
        "RegionsStatus": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/RegionsStatusDefinition"
            }
        },
        "ServiceName": {
            "type": "string",
            "description": "The id of the public cloud project. If omitted,\nthe `OVH_CLOUD_PROJECT_SERVICE` environment variable is used."
        },
        "Status": {
            "type": "string"
        },
        "Type": {
            "type": "string"
        },
        "VlanId": {
            "type": "number",
            "description": "a vlan id to associate with the network.\nChanging this value recreates the resource. Defaults to 0."
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "ServiceName"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id",
        "/properties/RegionsStatus",
        "/properties/Status",
        "/properties/Type"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    }
}