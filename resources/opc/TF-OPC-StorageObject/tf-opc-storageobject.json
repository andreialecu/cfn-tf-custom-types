{
    "typeName": "TF::OPC::StorageObject",
    "description": "Creates and manages a Object in an Oracle Cloud Infrastructure Storage Classic container. `storage_endpoint` must be set in the provider or environment to manage these resources.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/opc/TF-OPC-StorageObject/docs/README.md",
    "definitions": {
        "MetadataDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "MapKey": {
                    "type": "string"
                },
                "MapValue": {
                    "type": "string"
                }
            },
            "required": [
                "MapKey",
                "MapValue"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AcceptRanges": {
            "type": "string"
        },
        "Container": {
            "type": "string",
            "description": "The name of Storage Container the store the object in."
        },
        "Content": {
            "type": "string",
            "description": "Raw content in string-form of the data."
        },
        "ContentDisposition": {
            "type": "string",
            "description": "Set the HTTP `Content-Disposition` header to specify the override behaviour for the browser, e.g. `inline` or `attachment`."
        },
        "ContentEncoding": {
            "type": "string",
            "description": "set the HTTP `Content-Encoding` for the object."
        },
        "ContentLength": {
            "type": "number"
        },
        "ContentType": {
            "type": "string",
            "description": "set the MIME type for the object."
        },
        "CopyFrom": {
            "type": "string",
            "description": "name of an existing object used to create the new object as a copy. The value is in form `container/object`. You must UTF-8-encode and then URL-encode the names of the container and object."
        },
        "DeleteAt": {
            "type": "number",
            "description": "The date and time in UNIX Epoch time stamp format when the system removes the object."
        },
        "Etag": {
            "type": "string",
            "description": "MD5 checksum value of the request body. Strongly Recommended."
        },
        "File": {
            "type": "string",
            "description": "File path for the content to use for data."
        },
        "Id": {
            "type": "string"
        },
        "LastModified": {
            "type": "string"
        },
        "Metadata": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "$ref": "#/definitions/MetadataDefinition"
            },
            "description": "Additional object metadata headers. See [Object Metadata ](#object-metadata) below for more information."
        },
        "Name": {
            "type": "string",
            "description": "The name of the Storage Object."
        },
        "ObjectManifest": {
            "type": "string"
        },
        "Timestamp": {
            "type": "string"
        },
        "TransactionId": {
            "type": "string"
        },
        "TransferEncoding": {
            "type": "string",
            "description": "Set to `chunked` to enable chunked transfer encoding."
        }
    },
    "additionalProperties": false,
    "required": [
        "Container",
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/AcceptRanges",
        "/properties/ContentLength",
        "/properties/Id",
        "/properties/LastModified",
        "/properties/ObjectManifest",
        "/properties/Timestamp",
        "/properties/TransactionId"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    }
}