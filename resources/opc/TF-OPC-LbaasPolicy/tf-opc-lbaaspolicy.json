        "TrustedCertificatePolicyDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "TrustedCertificate": {
                    "type": "string"
                }
            },
            "required": [
                "TrustedCertificate"
            ]
        }
