            "required": [
                "RedirectUri",
                "ResponseCode"
            ]
        },
        "ResourceAccessControlPolicyDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "DeniedClients": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    }
