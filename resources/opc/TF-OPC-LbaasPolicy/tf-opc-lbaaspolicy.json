            ]
        },
        "SetRequestHeaderPolicyDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "ActionWhenHeaderExists": {
                    "type": "string"
                },
                "ActionWhenHeaderValueIs": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    }
                },
                "ActionWhenHeaderValueIsNot": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    }
                },
                "HeaderName": {
                    "type": "string"
                },
                "Value": {
                    "type": "string"
                }
            },
            "required": [
                "HeaderName"
            ]
