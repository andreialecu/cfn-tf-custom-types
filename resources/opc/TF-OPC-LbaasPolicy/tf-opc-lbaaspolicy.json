            "type": "object",
            "additionalProperties": false,
            "properties": {
                "CookieName": {
                    "type": "string"
                }
            },
            "required": [
                "CookieName"
            ]
        },
        "CloudgatePolicyDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "CloudgateApplication": {
                    "type": "string"
                },
                "CloudgatePolicyName": {
                    "type": "string"
                },
                "IdentityServiceInstanceGuid": {
                    "type": "string"
                },
                "VirtualHostnameForPolicyAttribution": {
                    "type": "string"
                }
            },
            "required": [
                "VirtualHostnameForPolicyAttribution"
            ]
        },
        "LoadBalancerCookieStickinessPolicyDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "CookieExpirationPeriod": {
                    "type": "number"
                }
            },
            "required": [
                "CookieExpirationPeriod"
            ]
        },
        "LoadBalancingMechanismPolicyDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "LoadBalancingMechanism": {
                    "type": "string"
                }
            },
            "required": [
                "LoadBalancingMechanism"
            ]
        },
        "RateLimitingRequestPolicyDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "BurstSize": {
                    "type": "number"
