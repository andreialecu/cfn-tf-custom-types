{
    "typeName": "TF::Panos::PanoramaGreTunnel",
    "description": "This resource allows you to add/update/delete Panorama GRE tunnels.\n\n*Minimum PAN-OS version*: 9.0",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/panos/TF-Panos-PanoramaGreTunnel/docs/README.md",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "CopyTos": {
            "type": "boolean",
            "description": "Copy IP TOS bits from inner packet to GRE packet."
        },
        "Disabled": {
            "type": "boolean",
            "description": "Disable the GRE tunnel."
        },
        "EnableKeepAlive": {
            "type": "boolean",
            "description": "Enable tunnel monitoring."
        },
        "Id": {
            "type": "string"
        },
        "Interface": {
            "type": "string",
            "description": "Interface to terminate tunnel."
        },
        "KeepAliveHoldTimer": {
            "type": "number",
            "description": "Keep alive hold timer."
        },
        "KeepAliveInterval": {
            "type": "number",
            "description": "Keep alive interval."
        },
        "KeepAliveRetry": {
            "type": "number",
            "description": "Keep alive retry."
        },
        "LocalAddressType": {
            "type": "string",
            "description": "Type of local address.  Valid values are\n`ip` (default) or `floating-ip`."
        },
        "LocalAddressValue": {
            "type": "string",
            "description": "IP address value."
        },
        "Name": {
            "type": "string",
            "description": "The GRE tunnel name."
        },
        "PeerAddress": {
            "type": "string",
            "description": "Peer IP address."
        },
        "Template": {
            "type": "string",
            "description": "The template name."
        },
        "Ttl": {
            "type": "number",
            "description": "Time to live."
        },
        "TunnelInterface": {
            "type": "string",
            "description": "Tunnel interface to apply the GRE tunnel to."
        }
    },
    "additionalProperties": false,
    "required": [
        "Interface",
        "LocalAddressValue",
        "Name",
        "PeerAddress",
        "Template",
        "TunnelInterface"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    }
}