            ]
        },
        "DynamicIpAndPortDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "InterfaceAddress": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/InterfaceAddressDefinition"
                    },
                    "maxItems": 1
                },
                "TranslatedAddress": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/TranslatedAddressDefinition"
                    },
                    "maxItems": 1
                }
            },
            "required": []
