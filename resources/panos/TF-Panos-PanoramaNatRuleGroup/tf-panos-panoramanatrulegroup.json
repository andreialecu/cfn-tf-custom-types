{
    "typeName": "TF::Panos::PanoramaNatRuleGroup",
    "description": "This resource allows you to add/update/delete a group of Panorama NAT rules.\n\nThis resource manages clusters of NAT rules in a single device group,\nenforcing both the contents of individual rules as well as their\nordering.  Rules are defined in a `rule` config block.\n\nAlthough you cannot modify non-group NAT rules with this\nresource, the `position_keyword` and `position_reference` parameters allow you\nto reference some other NAT rule that already exists, using it as\na means to ensure some rough placement within the ruleset as a whole.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/panos/TF-Panos-PanoramaNatRuleGroup/docs/README.md",
    "definitions": {
        "RuleDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Description": {
                    "type": "string",
                    "description": "The description."
                },
                "Disabled": {
                    "type": "boolean",
                    "description": "Set to `true` to disable this rule."
                },
                "Name": {
                    "type": "string",
                    "description": "The NAT rule's name."
                },
                "NegateTarget": {
                    "type": "boolean",
                    "description": "Instead of applying the rule for the\ngiven serial numbers, apply it to everything except them."
                },
                "Tags": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "type": "string"
                    },
                    "description": "List of administrative tags."
                },
                "Type": {
                    "type": "string",
                    "description": ". NAT type.  This can be `ipv4` (default), `nat64`, or\n`nptv6`."
                },
                "OriginalPacket": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/OriginalPacketDefinition"
                    },
                    "maxItems": 1,
                    "minItems": 1
                },
                "Target": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "$ref": "#/definitions/TargetDefinition"
                    }
                },
                "TranslatedPacket": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/TranslatedPacketDefinition"
                    },
                    "maxItems": 1,
                    "minItems": 1
                }
            },
            "required": [
                "Name"
            ]
        },
        "OriginalPacketDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "DestinationAddresses": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    },
                    "description": "List of destination address(es)."
                },
                "DestinationInterface": {
                    "type": "string",
                    "description": "Egress interface from route lookup (default:\n`any`)."
                },
                "DestinationZone": {
                    "type": "string",
                    "description": "The destination zone."
                },
                "Service": {
                    "type": "string",
                    "description": "Service (default: `any`)."
                },
                "SourceAddresses": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    },
                    "description": "List of source address(es)."
                },
                "SourceZones": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    },
                    "description": "The list of source zone(s)."
                }
            },
            "required": [
                "DestinationAddresses",
                "DestinationZone",
                "SourceAddresses",
                "SourceZones"
            ]
        },
        "TargetDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Serial": {
                    "type": "string",
                    "description": "The serial number of the firewall."
                },
                "VsysList": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    },
                    "description": "A subset of all available vsys on the firewall\nthat should be in this device group.  If the firewall is a virtual firewall,\nthen this parameter should just be omitted."
                }
            },
            "required": [
                "Serial"
            ]
        },
        "TranslatedPacketDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Destination": {
                    "type": "array",
