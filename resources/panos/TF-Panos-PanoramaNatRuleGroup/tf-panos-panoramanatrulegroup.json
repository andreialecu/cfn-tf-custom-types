                    "type": "string"
                },
                "Port": {
                    "type": "number"
                }
            },
            "required": [
                "Address"
            ]
        },
        "DynamicIpDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "TranslatedAddresses": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
                    }
                },
                "Fallback": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/FallbackDefinition"
                    },
                    "maxItems": 1
                }
            },
            "required": [
