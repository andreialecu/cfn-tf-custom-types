                },
                "Static": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "$ref": "#/definitions/StaticDefinition"
                    },
                    "maxItems": 1
                },
                "StaticTranslation": {
                    "type": "array",
                    "insertionOrder": true,
