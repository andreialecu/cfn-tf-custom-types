{
    "typeName": "TF::Panos::PanoramaTemplateStackEntry",
    "description": "This resource allows you to add/update/delete a specific device in a Panorama\ntemplate stack.\n\nThis resource has some overlap with the `panos_panorama_template_stack`\nresource.  If you want to use this resource with the other one, then make\nsure that your `panos_panorama_template_stack` spec does not define the\n`devices` field.\n\nThis is the appropriate resource to use if you have a pre-existing template stack\nin Panorama and don't want Terraform to delete it on `terraform destroy`.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/panos/TF-Panos-PanoramaTemplateStackEntry/docs/README.md",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "Device": {
            "type": "string",
            "description": "The serial number of the device to add."
        },
        "Id": {
            "type": "string"
        },
        "TemplateStack": {
            "type": "string",
            "description": "The template name."
        }
    },
    "additionalProperties": false,
    "required": [
        "Device",
        "TemplateStack"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    }
}