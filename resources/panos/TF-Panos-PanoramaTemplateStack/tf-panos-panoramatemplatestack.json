{
    "typeName": "TF::Panos::PanoramaTemplateStack",
    "description": "This resource allows you to add/update/delete Panorama template stacks.\n\nThis resource has some overlap with the `panos_panorama_template_stack_entry`\nresource.  If you want to use this resource with the other one, then make\nsure that your `panos_panorama_template_stack` spec does not define any\n`device` blocks, and just stays as \"computed\".\n\nThis is the appropriate resource to use if `terraform destroy` should delete\nthe template stack.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/panos/TF-Panos-PanoramaTemplateStack/docs/README.md",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "DefaultVsys": {
            "type": "string",
            "description": "The default virtual system template configuration\npushed to firewalls with a single virtual system.  **Note** - you can only\nset this if there is at least one template in this stack."
        },
        "Description": {
            "type": "string",
            "description": "The stack's description."
        },
        "Devices": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "List of serial numbers to include in this stack."
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The stack's name."
        },
        "Templates": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "type": "string"
            },
            "description": "List of templates in this stack."
        }
    },
    "additionalProperties": false,
    "required": [
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    }
}