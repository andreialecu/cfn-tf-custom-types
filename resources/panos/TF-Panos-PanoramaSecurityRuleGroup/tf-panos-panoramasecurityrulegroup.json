                    "items": {
                        "type": "string"
                    },
                    "description": "List of source users."
                },
                "SourceZones": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "type": "string"
                    },
                    "description": "List of source zones."
                },
                "Spyware": {
                    "type": "string",
                    "description": "Profile Setting: `Profiles` - The anti-spyware\nsetting."
                },
                "Tags": {
                    "type": "array",
                    "insertionOrder": true,
                    "items": {
                        "type": "string"
