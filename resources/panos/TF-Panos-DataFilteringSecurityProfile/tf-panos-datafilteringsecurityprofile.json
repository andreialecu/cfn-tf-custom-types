{
    "typeName": "TF::Panos::DataFilteringSecurityProfile",
    "description": "Manages data filtering security profiles.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/panos/TF-Panos-DataFilteringSecurityProfile/docs/README.md",
    "definitions": {
        "RuleDefinition": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "AlertThreshold": {
                    "type": "number",
                    "description": "Alert threshold."
                },
                "Applications": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "type": "string"
                    },
                    "description": "List of applications."
                },
                "BlockThreshold": {
                    "type": "number",
                    "description": "Block threshold."
                },
                "DataPattern": {
                    "type": "string",
                    "description": "The data pattern name."
                },
                "Direction": {
                    "type": "string",
                    "description": "Direction.  Valid values are `both` (default),\n`download`, or `upload`."
                },
                "FileTypes": {
                    "type": "array",
                    "insertionOrder": false,
                    "items": {
                        "type": "string"
                    },
                    "description": "List of file types."
                },
                "LogSeverity": {
                    "type": "string",
                    "description": "Log severity."
                }
            },
            "required": [
                "DataPattern"
            ]
        }
    },
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "DataCapture": {
            "type": "boolean",
            "description": "Data capture."
        },
        "Description": {
            "type": "string",
            "description": "The description."
        },
        "DeviceGroup": {
            "type": "string",
            "description": "The device group location (default: `shared`)."
        },
        "Id": {
            "type": "string"
        },
        "Name": {
            "type": "string",
            "description": "The name."
        },
        "Vsys": {
            "type": "string",
            "description": "The vsys location (default: `vsys1`)."
        },
        "Rule": {
            "type": "array",
            "insertionOrder": false,
            "items": {
                "$ref": "#/definitions/RuleDefinition"
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "Name"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    }
}