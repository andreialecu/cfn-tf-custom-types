{
    "typeName": "TF::Panos::OspfAreaInterface",
    "description": "Manages an OSPF area interface.",
    "sourceUrl": "https://github.com/iann0036/cfn-tf-custom-types.git",
    "documentationUrl": "https://github.com/iann0036/cfn-tf-custom-types/blob/docs/resources/panos/TF-Panos-OspfAreaInterface/docs/README.md",
    "definitions": {},
    "properties": {
        "tfcfnid": {
            "description": "Internal identifier for tracking resource changes. Do not use.",
            "type": "string"
        },
        "AuthProfile": {
            "type": "string",
            "description": "Auth profile."
        },
        "BfdProfile": {
            "type": "string",
            "description": "BFD profile."
        },
        "DeadCounts": {
            "type": "number",
            "description": "Dead counts (default: `4`)."
        },
        "Enable": {
            "type": "boolean",
            "description": "Enable (default: `true`)."
        },
        "GraceRestartDelay": {
            "type": "number",
            "description": "Graceful restart hello delay in\nseconds (default: `10`)."
        },
        "HelloInterval": {
            "type": "number",
            "description": "Hello interval in seconds (default: `10`)."
        },
        "Id": {
            "type": "string"
        },
        "LinkType": {
            "type": "string",
            "description": "Link type.  Valid values are `broadcast` (default),\n`p2p`, or `p2mp`."
        },
        "Metric": {
            "type": "number",
            "description": "Metric (default: `10`)."
        },
        "Name": {
            "type": "string",
            "description": "Interface name."
        },
        "Neighbors": {
            "type": "array",
            "insertionOrder": true,
            "items": {
                "type": "string"
            },
            "description": "(p2mp) List of neighbors."
        },
        "OspfArea": {
            "type": "string",
            "description": "OSPF area name."
        },
        "Passive": {
            "type": "boolean",
            "description": "Passive."
        },
        "Priority": {
            "type": "number",
            "description": "Priority (default: `1`)."
        },
        "RetransmitInterval": {
            "type": "number",
            "description": "Retransmit interval in seconds (default: `5`)."
        },
        "Template": {
            "type": "string",
            "description": "The template location."
        },
        "TemplateStack": {
            "type": "string"
        },
        "TransitDelay": {
            "type": "number",
            "description": "Transit delay in seconds (default: `1`)."
        },
        "VirtualRouter": {
            "type": "string",
            "description": "The virtual router name."
        }
    },
    "additionalProperties": false,
    "required": [
        "Name",
        "OspfArea",
        "VirtualRouter"
    ],
    "readOnlyProperties": [
        "/properties/tfcfnid",
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/tfcfnid"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "read": {
            "permissions": [
                "s3:GetObject"
            ]
        },
        "update": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "delete": {
            "permissions": [
                "s3:GetObject",
                "s3:DeleteObject",
                "lambda:InvokeFunction"
            ]
        },
        "list": {
            "permissions": [
                "s3:GetObject",
                "s3:ListBucket"
            ]
        }
    }
}